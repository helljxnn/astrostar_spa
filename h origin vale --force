[32m+[m[32mimport { useState } from "react";[m
[32m+[m[32mimport { motion, AnimatePresence } from "framer-motion";[m
[32m+[m[32mimport { FaChevronDown, FaTimes } from "react-icons/fa";[m
[32m+[m
[32m+[m[32mexport const ScheduleSelector = ({ value = [], onChange, error, touched }) => {[m
[32m+[m[32m  const [isOpen, setIsOpen] = useState(false);[m
[32m+[m
[32m+[m[32m  // Horarios disponibles[m
[32m+[m[32m  const schedules = [[m
[32m+[m[32m    "6:00 AM - 8:00 AM",[m
[32m+[m[32m    "8:00 AM - 10:00 AM",[m
[32m+[m[32m    "10:00 AM - 12:00 PM",[m
[32m+[m[32m    "2:00 PM - 4:00 PM",[m
[32m+[m[32m    "4:00 PM - 6:00 PM",[m
[32m+[m[32m    "6:00 PM - 8:00 PM",[m
[32m+[m[32m  ];[m
[32m+[m
[32m+[m[32m  // Alternar selecci√≥n de horario[m
[32m+[m[32m  const toggleSchedule = (schedule) => {[m
[32m+[m[32m    if (value.includes(schedule)) {[m
[32m+[m[32m      onChange(value.filter((item) => item !== schedule));[m
[32m+[m[32m    } else {[m
[32m+[m[32m      onChange([...value, schedule]);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="flex flex-col relative">[m
[32m+[m[32m      <label className="mb-1 font-semibold text-gray-700 text-lg">[m
[32m+[m[32m        Seleccionar horarios[m
[32m+[m[32m      </label>[m
[32m+[m
[32m+[m[32m      {/* Caja principal */}[m
[32m+[m[32m      <div[m
[32m+[m[32m        onClick={() => setIsOpen(!isOpen)}[m
[32m+[m[32m        className="border border-gray-300 rounded-xl p-3 bg-white shadow-md cursor-pointer[m[41m [m
[32m+[m[32m                   flex justify-between items-center flex-wrap gap-2[m[41m [m
[32m+[m[32m                   focus-within:ring-2 focus-within:ring-[#9BE9FF] focus-within:border-[#9BE9FF]"[m
[32m+[m[32m      >[m
[32m+[m[32m        <div className="flex flex-wrap gap-2">[m
[32m+[m[32m          {value.length > 0 ? ([m
[32m+[m[32m            value.map((schedule) => ([m
[32m+[m[32m              <motion.span[m
[32m+[m[32m                key={schedule}[m
[32m+[m[32m                layout[m
[32m+[m[32m                className="flex items-center gap-1 px-3 py-1 bg-[#9BE9FF]/80 text-gray-900 text-sm font-medium rounded-full shadow-sm"[m
[32m+[m[32m              >[m
[32m+[m[32m                {schedule}[m
[32m+[m[32m                <FaTimes[m
[32m+[m[32m                  className="cursor-pointer text-gray-700 hover:text-red-500"[m
[32m+[m[32m                  onClick={(e) => {[m
[32m+[m[32m                    e.stopPropagation();[m
[32m+[m[32m                    toggleSchedule(schedule);[m
[32m+[m[32m                  }}[m
[32m+[m[32m                />[m
[32m+[m[32m              </motion.span>[m
[32m+[m[32m            ))[m
[32m+[m[32m          ) : ([m
[32m+[m[32m            <span className="text-gray-400 text-sm">[m
[32m+[m[32m              Selecciona horarios disponibles[m
[32m+[m[32m            </span>[m
[32m+[m[32m          )}[m
[32m+[m[32m        </div>[m
[32m+[m[32m        <FaChevronDown[m
[32m+[m[32m          className={`text-gray-500 transition-transform ${[m
[32m+[m[32m            isOpen ? "rotate-180" : ""[m
[32m+[m[32m          }`}[m
[32m+[m[32m        />[m
[32m+[m[32m      </div>[m
[32m+[m
[32m+[m[32m      {/* Dropdown animado */}[m
[32m+[m[32m      <AnimatePresence>[m
[32m+[m[32m        {isOpen && ([m
[32m+[m[32m          <motion.div[m
[32m+[m[32m            initial={{ opacity: 0, y: -10 }}[m
[32m+[m[32m            animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m            exit={{ opacity: 0, y: -10 }}[m
[32m+[m[32m            className="absolute top-full mt-2 w-full bg-white border border-gray-200 rounded-xl shadow-lg z-10 overflow-hidden"[m
[32m+[m[32m          >[m
[32m+[m[32m            {schedules.map((schedule) => ([m
[32m+[m[32m              <div[m
[32m+[m[32m                key={schedule}[m
[32m+[m[32m                onClick={() => toggleSchedule(schedule)}[m
[32m+[m[32m                className={`px-4 py-2 cursor-pointer text-sm hover:bg-[#e6faff] transition[m[41m [m
[32m+[m[32m                  ${[m
[32m+[m[32m                    value.includes(schedule)[m
[32m+[m[32m                      ? "bg-[#9BE9FF]/30 font-medium"[m
[32m+[m[32m                      : "text-gray-700"[m
[32m+[m[32m                  }`}[m
[32m+[m[32m              >[m
[32m+[m[32m                {schedule}[m
[32m+[m[32m              </div>[m
[32m+[m[32m            ))}[m
[32m+[m[32m          </motion.div>[m
[32m+[m[32m        )}[m
[32m+[m[32m      </AnimatePresence>[m
[32m+[m
[32m+[m[32m      {/* Error de validaci√≥n */}[m
[32m+[m[32m      {error && touched && ([m
[32m+[m[32m        <span className="text-sm text-red-500 mt-1">{error}</span>[m
[32m+[m[32m      )}[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[1mdiff --git a/src/features/dashboard/pages/Admin/pages/Services/EmployeesSchedule/components/RecurrenceGenerator.jsx b/src/features/dashboard/pages/Admin/pages/Services/EmployeesSchedule/components/RecurrenceGenerator.jsx[m
[1mnew file mode 100644[m
[1mindex 0000000..3326bc1[m
[1m--- /dev/null[m
[1m+++ b/src/features/dashboard/pages/Admin/pages/Services/EmployeesSchedule/components/RecurrenceGenerator.jsx[m
[36m@@ -0,0 +1,96 @@[m
[32m+[m[32m// RecurrenceGenerator.jsx[m
[32m+[m[32mimport { addDays, addWeeks, addMonths, addYears, isBefore } from "date-fns";[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Genera los horarios repetidos seg√∫n la opci√≥n seleccionada[m
[32m+[m[32m * @param {Object} schedule Horario base[m
[32m+[m[32m * @param {Date} endDate Fecha l√≠mite para generar recurrencias (ej: +30 d√≠as)[m
[32m+[m[32m * @returns {Array} Lista de horarios expandidos[m
[32m+[m[32m */[m
[32m+[m[32mexport function generateRecurringSchedules([m
[32m+[m[32m  schedule,[m
[32m+[m[32m  endDate = addMonths(new Date(), 1)[m
[32m+[m[32m) {[m
[32m+[m[32m  const schedules = [];[m
[32m+[m
[32m+[m[32m  const start = new Date(`${schedule.fecha}T${schedule.horaInicio}`);[m
[32m+[m[32m  const end = new Date(`${schedule.fecha}T${schedule.horaFin}`);[m
[32m+[m
[32m+[m[32m  const intervalo = schedule.intervalo || 1;[m
[32m+[m
[32m+[m[32m  // siempre guardar el primero[m
[32m+[m[32m  schedules.push({[m
[32m+[m[32m    ...schedule,[m
[32m+[m[32m    start,[m
[32m+[m[32m    end,[m
[32m+[m[32m    title: `${schedule.empleado} - ${schedule.cargo}`,[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  let cursor = start;[m
[32m+[m
[32m+[m[32m  switch (schedule.repeticion) {[m
[32m+[m[32m    case "dia":[m
[32m+[m[32m      while (isBefore(cursor, endDate)) {[m
[32m+[m[32m        cursor = addDays(cursor, intervalo);[m
[32m+[m[32m        schedules.push(makeSchedule(schedule, cursor, start, end));[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    case "semana":[m
[32m+[m[32m      while (isBefore(cursor, endDate)) {[m
[32m+[m[32m        cursor = addWeeks(cursor, intervalo);[m
[32m+[m[32m        schedules.push(makeSchedule(schedule, cursor, start, end));[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    case "mes":[m
[32m+[m[32m      while (isBefore(cursor, endDate)) {[m
[32m+[m[32m        cursor = addMonths(cursor, intervalo);[m
[32m+[m[32m        schedules.push(makeSchedule(schedule, cursor, start, end));[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    case "anio":[m
[32m+[m[32m      while (isBefore(cursor, endDate)) {[m
[32m+[m[32m        cursor = addYears(cursor, intervalo);[m
[32m+[m[32m        schedules.push(makeSchedule(schedule, cursor, start, end));[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    case "laboral":[m
[32m+[m[32m      while (isBefore(cursor, endDate)) {[m
[32m+[m[32m        cursor = addDays(cursor, 1);[m
[32m+[m[32m        if (cursor.getDay() >= 1 && cursor.getDay() <= 5) {[m
[32m+[m[32m          schedules.push(makeSchedule(schedule, cursor, start, end));[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    case "personalizado":[m
[32m+[m[32m      const { dias = [], repeticiones = 10 } = schedule;[m
[32m+[m[32m      let count = 1;[m
[32m+[m
[32m+[m[32m      while (count < repeticiones) {[m
[32m+[m[32m        cursor = addDays(cursor, 1);[m
[32m+[m[32m        if (dias.includes(cursor.getDay())) {[m
[32m+[m[32m          schedules.push(makeSchedule(schedule, cursor, start, end));[m
[32m+[m[32m          count++;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    default:[m
[32m+[m[32m      break;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return schedules;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction makeSchedule(schedule, cursor, start, end) {[m
[32m+[m[32m  return {[m
[32m+[m[32m    ...schedule,[m
[32m+[m[32m    start: new Date(cursor),[m
[32m+[m[32m    end: new Date(cursor.getTime() + (end - start)),[m
[32m+[m[32m    title: `${schedule.empleado} - ${schedule.cargo}`,[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[1mdiff --git a/src/features/dashboard/pages/Admin/pages/Services/EmployeesSchedule/hooks/useFormSchedulealidation.js b/src/features/dashboard/pages/Admin/pages/Services/EmployeesSchedule/hooks/useFormSchedulealidation.js[m
[1mnew file mode 100644[m
[1mindex 0000000..dd2a9c5[m
[1m--- /dev/null[m
[1m+++ b/src/features/dashboard/pages/Admin/pages/Services/EmployeesSchedule/hooks/useFormSchedulealidation.js[m
[36m@@ -0,0 +1,79 @@[m
[32m+[m[32m// useFormScheduleValidation.js[m
[32m+[m[32mimport { useState } from "react";[m
[32m+[m
[32m+[m[32mexport const useFormScheduleValidation = () => {[m
[32m+[m[32m  const [errors, setErrors] = useState({});[m
[32m+[m[32m  const [touched, setTouched] = useState({});[m
[32m+[m
[32m+[m[32m  const validateField = (name, value, formData) => {[m
[32m+[m[32m    let error = "";[m
[32m+[m
[32m+[m[32m    switch (name) {[m
[32m+[m[32m      case "empleado":[m
[32m+[m[32m        if (!value?.trim()) error = "El nombre del empleado es obligatorio.";[m
[32m+[m[32m        else if (value.length < 3)[m
[32m+[m[32m          error = "El nombre debe tener al menos 3 caracteres.";[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m      case "fecha":[m
[32m+[m[32m        if (!value) error = "La fecha es obligatoria.";[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m      case "horaInicio":[m
[32m+[m[32m        if (!value) error = "La hora de inicio es obligatoria.";[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m      case "horaFin":[m
[32m+[m[32m        if (!value) error = "La hora de finalizaci√≥n es obligatoria.";[m
[32m+[m[32m        else if (formData.horaInicio && value <= formData.horaInicio) {[m
[32m+[m[32m          error =[m
[32m+[m[32m            "La hora de finalizaci√≥n debe ser mayor que la hora de inicio.";[m
[32m+[m[32m        }[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m      case "area":[m
[32m+[m[32m        if (!value?.trim()) error = "El √°rea o departamento es obligatorio.";[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m      case "estado":[m
[32m+[m[32m        if (!value?.trim()) error = "Debe seleccionar un estado.";[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m      case "observaciones":[m
[32m+[m[32m        if (value && value.length < 5)[m
[32m+[m[32m          error = "Las observaciones deben tener al menos 5 caracteres.";[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m      default:[m
[32m+[m[32m        break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return error;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const validate = (formData) => {[m
[32m+[m[32m    const newErrors = {};[m
[32m+[m[32m    Object.keys(formData).forEach((key) => {[m
[32m+[m[32m      const error = validateField(key, formData[key], formData);[m
[32m+[m[32m      if (error) newErrors[key] = error;[m
[32m+[m[32m    });[m
[32m+[m[32m    setErrors(newErrors);[m
[32m+[m[32m    return Object.keys(newErrors).length === 0;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleBlur = (name, value, formData) => {[m
[32m+[m[32m    setTouched((prev) => ({ ...prev, [name]: true }));[m
[32m+[m[32m    const error = validateField(name, value, formData);[m
[32m+[m[32m    setErrors((prev) => ({ ...prev, [name]: error }));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const touchAllFields = (formData) => {[m
[32m+[m[32m    const allTouched = {};[m
[32m+[m[32m    Object.keys(formData).forEach((name) => {[m
[32m+[m[32m      allTouched[name] = true;[m
[32m+[m[32m    });[m
[32m+[m[32m    setTouched(allTouched);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return { errors, touched, validate, handleBlur, touchAllFields };[m
[32m+[m[32m};[m
