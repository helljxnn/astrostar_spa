[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  // Cargar datos del proveedor cuando se abra el modal en modo edici√≥n[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    if (isOpen && isEditing && providerToEdit) {[m
[32m+[m[32m      setValues({[m
[32m+[m[32m        tipoEntidad: providerToEdit.tipoEntidad || "juridica",[m
[32m+[m[32m        razonSocial: providerToEdit.razonSocial || "",[m
[32m+[m[32m        nit: providerToEdit.nit || "",[m
[32m+[m[32m        tipoProveedor: providerToEdit.tipoProveedor || "",[m
[32m+[m[32m        contactoPrincipal: providerToEdit.contactoPrincipal || "",[m
[32m+[m[32m        correo: providerToEdit.correo || "",[m
[32m+[m[32m        telefono: providerToEdit.telefono || "",[m
[32m+[m[32m        direccion: providerToEdit.direccion || "",[m
[32m+[m[32m        ciudad: providerToEdit.ciudad || "",[m
[32m+[m[32m        descripcion: providerToEdit.descripcion || "",[m
[32m+[m[32m        estado: providerToEdit.estado || "Activo",[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }, [isOpen, isEditing, providerToEdit, setValues]);[m
[32m+[m
[32m+[m[32m  const formatPhoneNumber = (phone) => {[m
[32m+[m[32m    if (!phone) return phone;[m
[32m+[m[32m    const cleanPhone = phone.replace(/[\s\-\(\)]/g, "");[m
[32m+[m[32m    if (cleanPhone.startsWith("+57") || cleanPhone.startsWith("57")) return phone;[m
[32m+[m[32m    if (/^\d{7,10}$/.test(cleanPhone)) return `+57 ${cleanPhone}`;[m
[32m+[m[32m    return phone;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleSubmit = async () => {[m
[32m+[m[32m    // 1. Marcar todos los campos como tocados[m
[32m+[m[32m    const allTouched = {};[m
[32m+[m[32m    Object.keys(providerValidationRules).forEach((field) => {[m
[32m+[m[32m      allTouched[field] = true;[m
[32m+[m[32m    });[m
[32m+[m[32m    setTouched(allTouched);[m
[32m+[m
[32m+[m[32m    // 2. Validar todos los campos[m
[32m+[m[32m    if (!validateAllFields()) {[m
[32m+[m[32m      if (isEditing) {[m
[32m+[m[32m        showErrorAlert([m
[32m+[m[32m          "Campos incompletos",[m
[32m+[m[32m          "Por favor completa todos los campos correctamente antes de continuar."[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // 3. Confirmar en modo edici√≥n[m
[32m+[m[32m    if (isEditing) {[m
[32m+[m[32m      const confirmResult = await showConfirmAlert([m
[32m+[m[32m        "¬øEst√°s seguro?",[m
[32m+[m[32m        `¬øDeseas actualizar la informaci√≥n del proveedor ${providerToEdit.razonSocial}?`,[m
[32m+[m[32m        {[m
[32m+[m[32m          confirmButtonText: "S√≠, actualizar",[m
[32m+[m[32m          cancelButtonText: "Cancelar",[m
[32m+[m[32m        }[m
[32m+[m[32m      );[m
[32m+[m[32m      if (!confirmResult.isConfirmed) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      const providerData = {[m
[32m+[m[32m        ...values,[m
[32m+[m[32m        telefono: formatPhoneNumber(values.telefono),[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      if (isEditing) {[m
[32m+[m[32m        const updatedProviderData = { ...providerData, id: providerToEdit.id };[m
[32m+[m[32m        await onUpdate(updatedProviderData);[m
[32m+[m[32m        showSuccessAlert([m
[32m+[m[32m          "Proveedor actualizado",[m
[32m+[m[32m          `Los datos de ${values.razonSocial} han sido actualizados exitosamente.`[m
[32m+[m[32m        );[m
[32m+[m[32m      } else {[m
[32m+[m[32m        await onSave(providerData);[m
[32m+[m[32m        showSuccessAlert([m
[32m+[m[32m          "Proveedor creado",[m
[32m+[m[32m          "El proveedor ha sido creado exitosamente."[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      resetForm();[m
[32m+[m[32m      onClose();[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m      console.error([m
[32m+[m[32m        `Error al ${isEditing ? "actualizar" : "crear"} proveedor:`,[m
[32m+[m[32m        error[m
[32m+[m[32m      );[m
[32m+[m[32m      showErrorAlert([m
[32m+[m[32m        "Error",[m
[32m+[m[32m        error.message ||[m
[32m+[m[32m          `Ocurri√≥ un error al ${isEditing ? "actualizar" : "crear"} el proveedor`[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Funci√≥n para cerrar el modal y resetear el formulario[m
[32m+[m[32m  const handleClose = () => {[m
[32m+[m[32m    resetForm();[m
[32m+[m[32m    onClose();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  if (!isOpen) return null;[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <motion.div[m
[32m+[m[32m      className="fixed inset-0 bg-black/60 backdrop-blur-sm flex items-center justify-center z-50 p-4"[m
[32m+[m[32m      initial={{ opacity: 0 }}[m
[32m+[m[32m      animate={{ opacity: 1 }}[m
[32m+[m[32m      exit={{ opacity: 0 }}[m
[32m+[m[32m    >[m
[32m+[m[32m      <motion.div[m
[32m+[m[32m        className="bg-white rounded-2xl shadow-2xl w-full max-w-4xl max-h-[90vh] overflow-y-auto relative"[m
[32m+[m[32m        initial={{ scale: 0.8, opacity: 0, y: 50 }}[m
[32m+[m[32m        animate={{ scale: 1, opacity: 1, y: 0 }}[m
[32m+[m[32m        exit={{ scale: 0.8, opacity: 0, y: 50 }}[m
[32m+[m[32m        transition={{ type: "spring", damping: 25, stiffness: 300 }}[m
[32m+[m[32m      >[m
[32m+[m[32m        {/* Header */}[m
[32m+[m[32m        <div className="sticky top-0 bg-white rounded-t-2xl border-b border-gray-200 p-6 z-10">[m
[32m+[m[32m          <button[m
[32m+[m[32m            className="absolute top-4 right-4 text-gray-400 hover:text-gray-600 transition-colors p-2 hover:bg-gray-100 rounded-full"[m
[32m+[m[32m            onClick={handleClose}[m
[32m+[m[32m          >[m
[32m+[m[32m            ‚úï[m
[32m+[m[32m          </button>[m
[32m+[m[32m          <h2 className="text-3xl font-bold bg-gradient-to-r from-primary-purple to-primary-blue bg-clip-text text-transparent text-center">[m
[32m+[m[32m            {isEditing ? "Editar Proveedor" : "Crear Proveedor"}[m
[32m+[m[32m          </h2>[m
[32m+[m[32m          {isEditing && ([m
[32m+[m[32m            <p className="text-center text-gray-600 mt-2">[m
[32m+[m[32m              Modificando informaci√≥n de:{" "}[m
[32m+[m[32m              <span className="font-semibold text-primary-purple">[m
[32m+[m[32m                {providerToEdit.razonSocial}[m
[32m+[m[32m              </span>[m
[32m+[m[32m            </p>[m
[32m+[m[32m          )}[m
[32m+[m[32m        </div>[m
[32m+[m
[32m+[m[32m        {/* Body */}[m
[32m+[m[32m        <div className="p-6 space-y-6">[m
[32m+[m[32m          {/* Tipo de Entidad - Radio Buttons Estilizados */}[m
[32m+[m[32m          <motion.div[m
[32m+[m[32m            initial={{ opacity: 0, y: 20 }}[m
[32m+[m[32m            animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m            transition={{ delay: 0.1 }}[m
[32m+[m[32m            className="space-y-4"[m
[32m+[m[32m          >[m
[32m+[m[32m            <label className="block text-sm font-medium text-gray-700 mb-4">[m
[32m+[m[32m              Tipo de Entidad *[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <div className="grid grid-cols-1 md:grid-cols-2 gap-4">[m
[32m+[m[32m              {entityTypes.map((type) => ([m
[32m+[m[32m                <motion.label[m
[32m+[m[32m                  key={type.value}[m
[32m+[m[32m                  className={`relative flex items-center p-4 rounded-xl border-2 cursor-pointer transition-all duration-300 ${[m
[32m+[m[32m                    values.tipoEntidad === type.value[m
[32m+[m[32m                      ? "border-primary-blue bg-gradient-to-br from-primary-blue/10 to-primary-purple/10 shadow-lg"[m
[32m+[m[32m                      : "border-gray-200 bg-white hover:border-gray-300 hover:shadow-md"[m
[32m+[m[32m                  }`}[m
[32m+[m[32m                  whileHover={{ scale: 1.02 }}[m
[32m+[m[32m                  whileTap={{ scale: 0.98 }}[m
[32m+[m[32m                >[m
[32m+[m[32m                  <input[m
[32m+[m[32m                    type="radio"[m
[32m+[m[32m                    name="tipoEntidad"[m
[32m+[m[32m                    value={type.value}[m
[32m+[m[32m                    checked={values.tipoEntidad === type.value}[m
[32m+[m[32m                    onChange={handleChange}[m
[32m+[m[32m                    className="sr-only"[m
[32m+[m[32m                  />[m
[32m+[m[41m                  [m
[32m+[m[32m                  {/* Custom Radio Button */}[m
[32m+[m[32m                  <div className="flex items-center">[m
[32m+[m[32m                    <div[m
[32m+[m[32m                      className={`w-6 h-6 rounded-full border-2 flex items-center justify-center transition-all duration-200 ${[m
[32m+[m[32m                        values.tipoEntidad === type.value[m
[32m+[m[32m                          ? "border-primary-blue bg-primary-blue shadow-lg"[m
[32m+[m[32m                          : "border-gray-300"[m
[32m+[m[32m                      }`}[m
[32m+[m[32m                    >[m
[32m+[m[32m                      {values.tipoEntidad === type.value && ([m
[32m+[m[32m                        <motion.div[m
[32m+[m[32m                          initial={{ scale: 0 }}[m
[32m+[m[32m                          animate={{ scale: 1 }}[m
[32m+[m[32m                          className="w-2.5 h-2.5 bg-white rounded-full"[m
[32m+[m[32m                        />[m
[32m+[m[32m                      )}[m
[32m+[m[32m                    </div>[m
[32m+[m[41m                    [m
[32m+[m[32m                    {/* Label con icono */}[m
[32m+[m[32m                    <div className="ml-4 flex items-center">[m
[32m+[m[32m                      <div className={`p-2 rounded-lg mr-3 ${[m
[32m+[m[32m                        values.tipoEntidad === type.value[m
[32m+[m[32m                          ? "bg-primary-blue/20 text-primary-blue"[m
[32m+[m[32m                          : "bg-gray-100 text-gray-600"[m
[32m+[m[32m                      }`}>[m
[32m+[m[32m                        {type.value === "juridica" ? ([m
[32m+[m[32m                          <svg className="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">[m
[32m+[m[32m                            <path strokeLinecap="round" strokeLinejoin="round" strokeWidth="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4" />[m
[32m+[m[32m                          </svg>[m
[32m+[m[32m                        ) : ([m
[32m+[m[32m                          <svg className="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">[m
[32m+[m[32m                            <path strokeLinecap="round" strokeLinejoin="round" strokeWidth="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z" />[m
[32m+[m[32m                          </svg>[m
[32m+[m[32m                        )}[m
[32m+[m[32m                      </div>[m
[32m+[m[32m                      <div>[m
[32m+[m[32m                        <span className={`text-lg font-semibold block ${[m
[32m+[m[32m                          values.tipoEntidad === type.value[m
[32m+[m[32m                            ? "text-primary-blue"[m
[32m+[m[32m                            : "text-gray-700"[m
[32m+[m[32m                        }`}>[m
[32m+[m[32m                          {type.label}[m
[32m+[m[32m                        </span>[m
[32m+[m[32m                        <span className="text-sm text-gray-500">[m
[32m+[m[32m                          {type.value === "juridica"[m[41m [m
[32m+[m[32m                            ? "Empresa o sociedad"[m[41m [m
[32m+[m[32m                            : "Persona individual"[m
[32m+[m[32m                          }[m
[32m+[m[32m                        </span>[m
[32m+[m[32m                      </div>[m
[32m+[m[32m                    </div>[m
[32m+[m[32m                  </div>[m
[32m+[m
[32m+[m[32m                  {/* Indicador de selecci√≥n */}[m
[32m+[m[32m                  {values.tipoEntidad === type.value && ([m
[32m+[m[32m                    <motion.div[m
[32m+[m[32m                      initial={{ scale: 0, rotate: 180 }}[m
[32m+[m[32m                      animate={{ scale: 1, rotate: 0 }}[m
[32m+[m[32m                      className="absolute top-2 right-2 text-primary-blue"[m
[32m+[m[32m                    >[m
[32m+[m[32m                      <svg className="w-5 h-5" fill="currentColor" viewBox="0 0 20 20">[m
[32m+[m[32m                        <path fillRule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clipRule="evenodd" />[m
[32m+[m[32m                      </svg>[m
[32m+[m[32m                    </motion.div>[m
[32m+[m[32m                  )}[m
[32m+[m[32m                </motion.label>[m
[32m+[m[32m              ))}[m
[32m+[m[32m            </div>[m
[32m+[m[32m          </motion.div>[m
[32m+[m
[32m+[m[32m          <div className="grid grid-cols-1 md:grid-cols-2 gap-6">[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label={values.tipoEntidad === "juridica" ? "Raz√≥n Social" : "Nombre Completo"}[m
[32m+[m[32m              name="razonSocial"[m
[32m+[m[32m              type="text"[m
[32m+[m[32m              placeholder={values.tipoEntidad === "juridica" ? "Nombre de la empresa" : "Nombre completo de la persona"}[m
[32m+[m[32m              value={values.razonSocial}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.razonSocial}[m
[32m+[m[32m              touched={touched.razonSocial}[m
[32m+[m[32m              delay={0.15}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label={values.tipoEntidad === "juridica" ? "NIT" : "N√∫mero de Identificaci√≥n"}[m
[32m+[m[32m              name="nit"[m
[32m+[m[32m              type="text"[m
[32m+[m[32m              placeholder={values.tipoEntidad === "juridica" ? "900123456-7" : "12345678"}[m
[32m+[m[32m              value={values.nit}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.nit}[m
[32m+[m[32m              touched={touched.nit}[m
[32m+[m[32m              delay={0.2}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label="Tipo de Proveedor"[m
[32m+[m[32m              name="tipoProveedor"[m
[32m+[m[32m              type="select"[m
[32m+[m[32m              placeholder="Selecciona el tipo de proveedor"[m
[32m+[m[32m              options={supplierTypes}[m
[32m+[m[32m              value={values.tipoProveedor}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.tipoProveedor}[m
[32m+[m[32m              touched={touched.tipoProveedor}[m
[32m+[m[32m              delay={0.25}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label="Contacto Principal"[m
[32m+[m[32m              name="contactoPrincipal"[m
[32m+[m[32m              type="text"[m
[32m+[m[32m              placeholder="Nombre del contacto principal"[m
[32m+[m[32m              value={values.contactoPrincipal}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.contactoPrincipal}[m
[32m+[m[32m              touched={touched.contactoPrincipal}[m
[32m+[m[32m              delay={0.3}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label="Correo Electr√≥nico"[m
[32m+[m[32m              name="correo"[m
[32m+[m[32m              type="email"[m
[32m+[m[32m              placeholder="correo@empresa.com"[m
[32m+[m[32m              value={values.correo}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.correo}[m
[32m+[m[32m              touched={touched.correo}[m
[32m+[m[32m              delay={0.35}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label="N√∫mero Telef√≥nico"[m
[32m+[m[32m              name="telefono"[m
[32m+[m[32m              type="text"[m
[32m+[m[32m              placeholder="3001234567 o 6012345678"[m
[32m+[m[32m              value={values.telefono}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.telefono}[m
[32m+[m[32m              touched={touched.telefono}[m
[32m+[m[32m              delay={0.4}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label="Direcci√≥n"[m
[32m+[m[32m              name="direccion"[m
[32m+[m[32m              type="text"[m
[32m+[m[32m              placeholder="Direcci√≥n completa"[m
[32m+[m[32m              value={values.direccion}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.direccion}[m
[32m+[m[32m              touched={touched.direccion}[m
[32m+[m[32m              delay={0.45}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label="Ciudad"[m
[32m+[m[32m              name="ciudad"[m
[32m+[m[32m              type="text"[m
[32m+[m[32m              placeholder="Ciudad donde opera"[m
[32m+[m[32m              value={values.ciudad}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.ciudad}[m
[32m+[m[32m              touched={touched.ciudad}[m
[32m+[m[32m              delay={0.5}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m
[32m+[m[32m            <div className="md:col-span-2">[m
[32m+[m[32m              <FormField[m
[32m+[m[32m                label="Descripci√≥n"[m
[32m+[m[32m                name="descripcion"[m
[32m+[m[32m                type="textarea"[m
[32m+[m[32m                placeholder="Descripci√≥n detallada del proveedor y sus servicios..."[m
[32m+[m[32m                value={values.descripcion}[m
[32m+[m[32m                onChange={handleChange}[m
[32m+[m[32m                onBlur={handleBlur}[m
[32m+[m[32m                error={errors.descripcion}[m
[32m+[m[32m                touched={touched.descripcion}[m
[32m+[m[32m                delay={0.55}[m
[32m+[m[32m                rows={4}[m
[32m+[m[32m              />[m
[32m+[m[32m            </div>[m
[32m+[m
[32m+[m[32m            <FormField[m
[32m+[m[32m              label="Estado"[m
[32m+[m[32m              name="estado"[m
[32m+[m[32m              type="select"[m
[32m+[m[32m              placeholder="Selecciona el estado"[m
[32m+[m[32m              options={states}[m
[32m+[m[32m              value={values.estado}[m
[32m+[m[32m              onChange={handleChange}[m
[32m+[m[32m              onBlur={handleBlur}[m
[32m+[m[32m              error={errors.estado}[m
[32m+[m[32m              touched={touched.estado}[m
[32m+[m[32m              delay={0.6}[m
[32m+[m[32m              required[m
[32m+[m[32m            />[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* Footer */}[m
[32m+[m[32m          <motion.div[m
[32m+[m[32m            initial={{ opacity: 0, y: 20 }}[m
[32m+[m[32m            animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m            transition={{ delay: 0.7 }}[m
[32m+[m[32m            className="flex justify-between pt-6 border-t border-gray-200"[m
[32m+[m[32m          >[m
[32m+[m[32m            <motion.button[m
[32m+[m[32m              type="button"[m
[32m+[m[32m              onClick={handleClose}[m
[32m+[m[32m              className="px-8 py-3 border-2 border-gray-300 text-gray-700 rounded-xl hover:bg-gray-50 transition-all duration-200 font-medium"[m
[32m+[m[32m              whileHover={{ scale: 1.02 }}[m
[32m+[m[32m              whileTap={{ scale: 0.98 }}[m
[32m+[m[32m            >[m
[32m+[m[32m              Cancelar[m
[32m+[m[32m            </motion.button>[m
[32m+[m
[32m+[m[32m            <motion.button[m
[32m+[m[32m              onClick={handleSubmit}[m
[32m+[m[32m              className="px-8 py-3 text-white rounded-xl transition-all duration-200 font-medium shadow-lg bg-gradient-to-r from-primary-purple to-primary-blue hover:from-primary-purple hover:to-primary-blue"[m
[32m+[m[32m              whileHover={{[m
[32m+[m[32m                scale: 1.02,[m
[32m+[m[32m                boxShadow: "0 10px 25px rgba(139, 92, 246, 0.3)",[m
[32m+[m[32m              }}[m
[32m+[m[32m              whileTap={{ scale: 0.98 }}[m
[32m+[m[32m            >[m
[32m+[m[32m              {isEditing ? "Actualizar Proveedor" : "Crear Proveedor"}[m
[32m+[m[32m            </motion.button>[m
[32m+[m[32m          </motion.div>[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </motion.div>[m
[32m+[m[32m    </motion.div>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default ProviderModal;[m
