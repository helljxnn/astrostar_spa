[32m+[m[32mimport React from "react";[m
[32m+[m
[32m+[m[32mconst Thead = ({ options }) => {[m
[32m+[m[32m  const { thead } = options;[m
[32m+[m[32m  const { titles = [], state = false } = thead;[m
[32m+[m
[32m+[m[32m  // Verificar si "Estado" y "Acciones" ya est√°n incluidos en los t√≠tulos[m
