[32m+[m[32mimport { IoMdDownload } from "react-icons/io";[m
[32m+[m
[32m+[m[32mconst SportsCategory = () => {[m
[32m+[m[32m  /* =====================[m
[32m+[m[32m        Estados[m
[32m+[m[32m  ===================== */[m
[32m+[m[32m  const [data, setData] = useState(SportsCategoryData);[m
[32m+[m[32m  const [searchTerm, setSearchTerm] = useState("");[m
[32m+[m[32m  const [currentPage, setCurrentPage] = useState(1);[m
[32m+[m[32m  const [isModalOpen, setIsModalOpen] = useState(false);[m
[32m+[m[41m  [m
[32m+[m[32m  // ‚úÖ Estados para manejo de edici√≥n[m
[32m+[m[32m  const [selectedCategory, setSelectedCategory] = useState(null);[m
[32m+[m[32m  const [isNew, setIsNew] = useState(true);[m
[32m+[m
[32m+[m[32m  const rowsPerPage = 8;[m
[32m+[m
[32m+[m[32m  /* =====================[m
[32m+[m[32m        Filtrado de b√∫squeda[m
[32m+[m[32m  ===================== */[m
[32m+[m[32m  const filteredData = useMemo(() => {[m
[32m+[m[32m    const term = searchTerm.toLowerCase();[m
[32m+[m[32m    return data.filter((item) =>[m
[32m+[m[32m      item.Nombre.toLowerCase().includes(term)[m
[32m+[m[32m    );[m
[32m+[m[32m  }, [data, searchTerm]);[m
[32m+[m
[32m+[m[32m  /* =====================[m
[32m+[m[32m        Acciones[m
[32m+[m[32m  ===================== */[m
[32m+[m[41m  [m
[32m+[m[32m  // ‚úÖ Funci√≥n para crear nueva categor√≠a[m
[32m+[m[32m  const handleCreate = () => {[m
[32m+[m[32m    setSelectedCategory(null); // No hay categor√≠a seleccionada[m
[32m+[m[32m    setIsNew(true); // Es una creaci√≥n nueva[m
[32m+[m[32m    setIsModalOpen(true); // Abrir modal[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ Funci√≥n para editar categor√≠a existente[m
[32m+[m[32m  const handleEdit = (item) => {[m
[32m+[m[32m    console.log("Editando categor√≠a:", item);[m
[32m+[m[32m    setSelectedCategory(item); // Pasar la categor√≠a seleccionada[m
[32m+[m[32m    setIsNew(false); // Es una edici√≥n[m
[32m+[m[32m    setIsModalOpen(true); // Abrir modal[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ Funci√≥n para eliminar con SweetAlert2 personalizado[m
[32m+[m[32m  const handleDelete = async (item) => {[m
[32m+[m[32m    try {[m
[32m+[m[32m      const result = await showDeleteAlert([m
[32m+[m[32m        "¬øEliminar categor√≠a?",[m
[32m+[m[32m        `¬øEst√°s seguro de que deseas eliminar la categor√≠a "${item.Nombre}"? Esta acci√≥n no se puede deshacer.`,[m
[32m+[m[32m        {[m
[32m+[m[32m          confirmButtonText: "S√≠, eliminar",[m
[32m+[m[32m          cancelButtonText: "Cancelar"[m
[32m+[m[32m        }[m
[32m+[m[32m      );[m
[32m+[m
[32m+[m[32m      if (result.isConfirmed) {[m
[32m+[m[32m        // Eliminar la categor√≠a de la lista[m
[32m+[m[32m        setData((prev) => prev.filter((cat) => cat.id !== item.id || cat.Nombre !== item.Nombre));[m
[32m+[m[41m        [m
[32m+[m[32m        // Mostrar mensaje de √©xito[m
[32m+[m[32m        await showSuccessAlert([m
[32m+[m[32m          "Categor√≠a eliminada",[m
[32m+[m[32m          `La categor√≠a "${item.Nombre}" ha sido eliminada exitosamente.`[m
[32m+[m[32m        );[m
[32m+[m[41m        [m
[32m+[m[32m        console.log("Categor√≠a eliminada:", item);[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m      console.error("Error al eliminar categor√≠a:", error);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ Funci√≥n para ver detalle[m
[32m+[m[32m  const handleView = (item) => {[m
[32m+[m[32m    console.log("Ver detalle de:", item);[m
[32m+[m[32m    alert(`Viendo detalles de: ${item.Nombre}\nEdad: ${item.EdadMinima} - ${item.EdadMaxima} a√±os\nEstado: ${item.Estado}`);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ Funci√≥n para lista detallada[m
[32m+[m[32m  const handleList = (item) => {[m
[32m+[m[32m    console.log("Ver lista detallada de:", item);[m
[32m+[m[32m    alert(`Lista detallada de: ${item.Nombre}`);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ Funci√≥n para guardar (crear o actualizar)[m
[32m+[m[32m  const handleSave = (categoryData) => {[m
[32m+[m[32m    if (isNew) {[m
[32m+[m[32m      // Crear nueva categor√≠a[m
[32m+[m[32m      console.log("Creando nueva categor√≠a:", categoryData);[m
[32m+[m[32m      setData((prev) => [...prev, categoryData]);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // Actualizar categor√≠a existente[m
[32m+[m[32m      console.log("Actualizando categor√≠a:", categoryData);[m
[32m+[m[32m      setData((prev) =>[m[41m [m
[32m+[m[32m        prev.map((cat) =>[m[41m [m
[32m+[m[32m          (cat.id === selectedCategory.id) || (cat.Nombre === selectedCategory.Nombre)[m
[32m+[m[32m            ? { ...categoryData, id: selectedCategory.id }[m
[32m+[m[32m            : cat[m
[32m+[m[32m        )[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ Funci√≥n para cerrar modal[m
[32m+[m[32m  const handleCloseModal = () => {[m
[32m+[m[32m    setIsModalOpen(false);[m
[32m+[m[32m    setSelectedCategory(null);[m
[32m+[m[32m    setIsNew(true);[m
[32m+[m[32m  };[m
[32m+[m
