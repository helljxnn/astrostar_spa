[32m+[m[32m            value={searchTerm}[m
[32m+[m[32m            onChange={(e) => setSearchTerm(e.target.value)}[m
[32m+[m[32m            placeholder="Buscar categor√≠a..."[m
[32m+[m[32m          />[m
[32m+[m
[32m+[m[32m          <div className="flex items-center gap-3">[m
[32m+[m[32m            <button className="flex items-center gap-2 px-4 py-2 rounded-lg text-gray-700 font-semibold hover:bg-gray-200 transition-colors">[m
[32m+[m[32m              <IoMdDownload size={22} className="text-primary-purple" />[m
[32m+[m[32m              Generar reporte[m
[32m+[m[32m            </button>[m
[32m+[m
[32m+[m[32m            <button[m
[32m+[m[32m              onClick={handleCreate}[m
[32m+[m[32m              className="flex items-center gap-2 px-4 py-2 bg-primary-blue text-white rounded-lg shadow hover:bg-primary-purple transition-colors"[m
[32m+[m[32m            >[m
[32m+[m[32m              <FaPlus /> Crear[m
[32m+[m[32m            </button>[m
[32m+[m[32m          </div>[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </div>[m
[32m+[m
[32m+[m[32m      {/* ==== Tabla con funciones completas ==== */}[m
[32m+[m[32m      <Table[m
[32m+[m[32m        thead={{[m
[32m+[m[32m          titles: ["Nombre", "Edad m√≠nima", "Edad m√°xima", "Estado", "Acciones"],[m
[32m+[m[32m          state: true,[m
[32m+[m[32m        }}[m
[32m+[m[32m        tbody={{[m
[32m+[m[32m          data: filteredData,[m
[32m+[m[32m          dataPropertys: ["Nombre", "EdadMinima", "EdadMaxima"],[m
[32m+[m[32m          state: true,[m
[32m+[m[32m        }}[m
[32m+[m[32m        onEdit={handleEdit} // ‚úÖ Funci√≥n completa para editar[m
[32m+[m[32m        onDelete={handleDelete} // ‚úÖ Funci√≥n completa para eliminar[m
