[32m+[m[32mconst messages = {[m
[32m+[m[32m  month: "Mes",[m
[32m+[m[32m  week: "Semana",[m
[32m+[m[32m  day: "D√≠a",[m
[32m+[m[32m  today: "Hoy",[m
[32m+[m[32m  previous: "Atr√°s",[m
[32m+[m[32m  next: "Siguiente",[m
[32m+[m[32m  noEventsInRange: "No hay horarios en este rango.",[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default function EmployeesScheduleCalendar({[m
[32m+[m[32m  schedules = [],[m
[32m+[m[32m  onOpenModalForSlot,[m
[32m+[m[32m  onOpenModalForEvent,[m
[32m+[m[32m}) {[m
[32m+[m[32m  const [view, setView] = useState("month");[m
[32m+[m[32m  const [date, setDate] = useState(new Date());[m
[32m+[m
[32m+[m[32m  const ScheduleEvent = ({ event }) => ([m
[32m+[m[32m    <div className="flex items-center gap-2 text-xs font-medium truncate cursor-pointer">[m
[32m+[m[32m      <span[m
[32m+[m[32m        className={`w-2 h-2 rounded-full ${event.color || "bg-primary-purple"}`}[m
[32m+[m[32m      />[m
[32m+[m[32m      <span className="truncate">{event.title}</span>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  const handleNavigate = (direction) => {[m
[32m+[m[32m    if (view === "month") {[m
[32m+[m[32m      setDate(direction === "next" ? addMonths(date, 1) : subMonths(date, 1));[m
[32m+[m[32m    } else if (view === "week") {[m
[32m+[m[32m      setDate(direction === "next" ? addWeeks(date, 1) : subWeeks(date, 1));[m
[32m+[m[32m    } else if (view === "day") {[m
[32m+[m[32m      setDate(direction === "next" ? addDays(date, 1) : subDays(date, 1));[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="w-full max-w-7xl mx-auto px-4 py-6 grid grid-cols-1 md:grid-cols-4 gap-6">[m
[32m+[m[32m      {/* Columna Izquierda */}[m
[32m+[m[32m      <div className="col-span-1 flex flex-col gap-6">[m
[32m+[m[32m        {/* Mini Calendario */}[m
[32m+[m[32m        <div className="rounded-2xl border border-gray-200 shadow-lg bg-white p-4">[m
[32m+[m[32m          <div className="flex items-center justify-between mb-3">[m
[32m+[m[32m            <button[m
[32m+[m[32m              onClick={() => setDate(subMonths(date, 1))}[m
[32m+[m[32m              className="p-1 rounded-full hover:bg-gray-200 transition"[m
[32m+[m[32m            >[m
[32m+[m[32m              <FaChevronLeft className="text-gray-600" />[m
[32m+[m[32m            </button>[m
[32m+[m[32m            <h3 className="text-lg font-bold text-gray-700 capitalize">[m
[32m+[m[32m              {format(date, "MMMM yyyy", { locale: es })}[m
[32m+[m[32m            </h3>[m
[32m+[m[32m            <button[m
[32m+[m[32m              onClick={() => setDate(addMonths(date, 1))}[m
[32m+[m[32m              className="p-1 rounded-full hover:bg-gray-200 transition"[m
[32m+[m[32m            >[m
[32m+[m[32m              <FaChevronRight className="text-gray-600" />[m
[32m+[m[32m            </button>[m
[32m+[m[32m          </div>[m
[32m+[m[32m          <div className="grid grid-cols-7 gap-2 text-center text-sm font-medium text-gray-500">[m
[32m+[m[32m            {["L", "M", "X", "J", "V", "S", "D"].map((d, i) => ([m
[32m+[m[32m              <div key={i}>{d}</div>[m
[32m+[m[32m            ))}[m
[32m+[m[32m          </div>[m
[32m+[m[32m          <div className="grid grid-cols-7 gap-2 mt-2 text-sm">[m
[32m+[m[32m            {Array.from([m
[32m+[m[32m              {[m
[32m+[m[32m                length: new Date([m
[32m+[m[32m                  date.getFullYear(),[m
[32m+[m[32m                  date.getMonth() + 1,[m
[32m+[m[32m                  0[m
[32m+[m[32m                ).getDate(),[m
[32m+[m[32m              },[m
[32m+[m[32m              (_, i) => {[m
[32m+[m[32m                const dayDate = new Date([m
[32m+[m[32m                  date.getFullYear(),[m
[32m+[m[32m                  date.getMonth(),[m
[32m+[m[32m                  i + 1[m
[32m+[m[32m                );[m
[32m+[m[32m                const isToday = isSameDay(dayDate, new Date());[m
[32m+[m[32m                return ([m
[32m+[m[32m                  <button[m
[32m+[m[32m                    key={i}[m
[32m+[m[32m                    onClick={() => setDate(dayDate)}[m
[32m+[m[32m                    className={`h-10 w-10 flex items-center justify-center rounded-full transition-all ${[m
[32m+[m[32m                      isToday[m
[32m+[m[32m                        ? "bg-primary-purple text-white font-bold shadow"[m
[32m+[m[32m                        : "hover:bg-gray-200 text-gray-700"[m
[32m+[m[32m                    }`}[m
[32m+[m[32m                  >[m
[32m+[m[32m                    {i + 1}[m
[32m+[m[32m                  </button>[m
[32m+[m[32m                );[m
[32m+[m[32m              }[m
[32m+[m[32m            )}[m
[32m+[m[32m          </div>[m
[32m+[m[32m        </div>[m
[32m+[m
[32m+[m[32m        {/* Lista de actividades */}[m
[32m+[m[32m        <div className="rounded-2xl border border-gray-200 shadow-lg bg-white p-4">[m
[32m+[m[32m          <h3 className="text-lg font-bold text-gray-700 mb-3">Actividades</h3>[m
[32m+[m[32m          <ul className="space-y-3 text-sm">[m
[32m+[m[32m            {schedules.length === 0 ? ([m
[32m+[m[32m              <li className="text-gray-400">No hay actividades</li>[m
[32m+[m[32m            ) : ([m
[32m+[m[32m              schedules.map((event, i) => ([m
[32m+[m[32m                <li[m
[32m+[m[32m                  key={i}[m
[32m+[m[32m                  className="flex items-center gap-2 bg-gray-50 rounded-lg px-3 py-2 hover:bg-gray-100 transition"[m
[32m+[m[32m                >[m
[32m+[m[32m                  <span[m
[32m+[m[32m                    className={`w-3 h-3 rounded-full ${[m
[32m+[m[32m                      event.color || "bg-primary-purple"[m
[32m+[m[32m                    }`}[m
[32m+[m[32m                  />[m
[32m+[m[32m                  <span className="font-medium text-gray-700">[m
[32m+[m[32m                    {event.title}[m
[32m+[m[32m                  </span>[m
[32m+[m[32m                  <span className="ml-auto text-xs text-gray-500">[m
[32m+[m[32m                    {format(event.start, "HH:mm", { locale: es })}[m
[32m+[m[32m                  </span>[m
[32m+[m[32m                </li>[m
[32m+[m[32m              ))[m
[32m+[m[32m            )}[m
[32m+[m[32m          </ul>[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </div>[m
[32m+[m
[32m+[m[32m      {/* Calendario Principal */}[m
[32m+[m[32m      <div className="col-span-3">[m
[32m+[m[32m        <motion.div[m
[32m+[m[32m          initial={{ opacity: 0, y: -15 }}[m
[32m+[m[32m          animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m          transition={{ duration: 0.4 }}[m
[32m+[m[32m          className="flex items-center justify-between mb-6"[m
[32m+[m[32m        >[m
[32m+[m[32m          <div className="flex items-center gap-3">[m
[32m+[m[32m            <button[m
[32m+[m[32m              onClick={() => handleNavigate("prev")}[m
[32m+[m[32m              className="p-2 rounded-full hover:bg-gray-200 transition"[m
[32m+[m[32m            >[m
[32m+[m[32m              <FaChevronLeft className="text-gray-600" />[m
[32m+[m[32m            </button>[m
[32m+[m[32m            <h2 className="text-3xl font-extrabold text-gray-800 tracking-tight capitalize">[m
[32m+[m[32m              {format(date, "MMMM yyyy", { locale: es })}[m
[32m+[m[32m            </h2>[m
[32m+[m[32m            <button[m
[32m+[m[32m              onClick={() => handleNavigate("next")}[m
[32m+[m[32m              className="p-2 rounded-full hover:bg-gray-200 transition"[m
[32m+[m[32m            >[m
[32m+[m[32m              <FaChevronRight className="text-gray-600" />[m
[32m+[m[32m            </button>[m
[32m+[m[32m          </div>[m
[32m+[m[32m          <div className="flex items-center gap-3">[m
[32m+[m[32m            {["month", "week", "day"].map((viewType) => ([m
[32m+[m[32m              <motion.button[m
[32m+[m[32m                key={viewType}[m
[32m+[m[32m                onClick={() => setView(viewType)}[m
[32m+[m[32m                whileHover={{ scale: 1.1 }}[m
[32m+[m[32m                whileTap={{ scale: 0.95 }}[m
[32m+[m[32m                className={`px-5 py-2 rounded-xl text-sm font-semibold shadow transition-all ${[m
[32m+[m[32m                  view === viewType[m
[32m+[m[32m                    ? "bg-primary-purple text-white shadow-md"[m
[32m+[m[32m                    : "bg-gray-100 text-gray-700 hover:bg-gray-200"[m
[32m+[m[32m                }`}[m
[32m+[m[32m              >[m
[32m+[m[32m                {messages[viewType]}[m
[32m+[m[32m              </motion.button>[m
[32m+[m[32m            ))}[m
[32m+[m[32m          </div>[m
[32m+[m[32m        </motion.div>[m
[32m+[m
[32m+[m[32m        <AnimatePresence mode="wait">[m
[32m+[m[32m          <motion.div[m
[32m+[m[32m            key={view}[m
[32m+[m[32m            initial={{ opacity: 0, y: 15 }}[m
[32m+[m[32m            animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m            exit={{ opacity: 0, y: -15 }}[m
[32m+[m[32m            transition={{ duration: 0.3 }}[m
[32m+[m[32m            className="rounded-2xl border border-gray-200 shadow-lg overflow-hidden bg-white"[m
