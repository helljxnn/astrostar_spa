[32m+[m
[32m+[m[32m// ================================[m
[32m+[m[32m// 2. Table.jsx (CORREGIDO)[m
[32m+[m[32m// ================================[m
[32m+[m[32mimport { useState } from "react";[m
[32m+[m[32mimport Thead from "./Thead";[m
[32m+[m[32mimport Tbody from "./Tbody";[m
[32m+[m[32mimport Pagination from "../../../../../../../../../shared/components/Table/Pagination";[m
[32m+[m
[32m+[m[32mconst Table = ({[m
[32m+[m[32m  thead,[m
[32m+[m[32m  tbody,[m
[32m+[m[32m  rowsPerPage = 10,[m
[32m+[m[32m  paginationFrom = 10,[m
[32m+[m[32m  onEdit,[m
[32m+[m[32m  onDelete,[m
[32m+[m[32m  onView,[m
[32m+[m[32m  onList,[m
[32m+[m[32m}) => {[m
[32m+[m[32m  /* =====================[m
[32m+[m[32m        Estado de la p√°gina[m
[32m+[m[32m  ===================== */[m
[32m+[m[32m  const [currentPage, setCurrentPage] = useState(1);[m
[32m+[m
[32m+[m[32m  const allData = tbody.data || [];[m
[32m+[m[32m  const totalRows = allData.length;[m
[32m+[m
[32m+[m[32m  /* =====================[m
[32m+[m[32m        Si no hay datos[m
[32m+[m[32m  ===================== */[m
[32m+[m[32m  if (totalRows === 0) {[m
[32m+[m[32m    return ([m
[32m+[m[32m      <div className="shadow-lg rounded-2xl bg-white flex flex-col border border-gray-200 overflow-hidden max-w-full">[m
[32m+[m[32m        <div className="p-8 text-center text-gray-400 italic">[m
[32m+[m[32m          No hay datos para mostrar.[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </div>[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /* =====================[m
[32m+[m[32m        Paginaci√≥n[m
[32m+[m[32m  ===================== */[m
[32m+[m[32m  const totalPages = Math.ceil(totalRows / rowsPerPage);[m
[32m+[m[32m  const showPagination = totalRows > paginationFrom && totalPages > 1;[m
[32m+[m
[32m+[m[32m  const handlePageChange = (page) => {[m
[32m+[m[32m    if (page >= 1 && page <= totalPages) {[m
[32m+[m[32m      setCurrentPage(page);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const startIndex = (currentPage - 1) * rowsPerPage;[m
[32m+[m[32m  const paginatedData = allData.slice(startIndex, startIndex + rowsPerPage);[m
[32m+[m
[32m+[m[32m  /* =====================[m
[32m+[m[32m        Render[m
[32m+[m[32m  ===================== */[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="shadow-lg rounded-2xl bg-white flex flex-col border border-gray-200 overflow-hidden max-w-full">[m
[32m+[m[32m      {/* ==== Tabla (desktop) ==== */}[m
[32m+[m[32m      <div className="overflow-x-auto hidden sm:block w-full scrollbar-thin scrollbar-thumb-gray-300 scrollbar-track-gray-100">[m
[32m+[m[32m        <table className="w-full border-collapse text-sm font-monserrat">[m
[32m+[m[32m          {/* ‚úÖ THEAD - mantiene estructura original */}[m
[32m+[m[32m          <Thead options={{ thead }} />[m
[32m+[m[41m          [m
[32m+[m[32m          {/* ‚úÖ TBODY - CORREGIDO: Props directas */}[m
[32m+[m[32m          <Tbody[m[41m [m
[32m+[m[32m            data={paginatedData}[m
[32m+[m[32m            dataPropertys={tbody.dataPropertys}[m
[32m+[m[32m            state={tbody.state}[m
[32m+[m[32m            onEdit={onEdit}[m
[32m+[m[32m            onDelete={onDelete}[m
[32m+[m[32m            onView={onView}[m
[32m+[m[32m            onList={onList}[m
[32m+[m[32m          />[m
[32m+[m[32m        </table>[m
[32m+[m[32m      </div>[m
[32m+[m
[32m+[m[32m      {/* ==== Tarjetas (m√≥vil) ==== */}[m
[32m+[m[32m      <div className="block sm:hidden p-3 space-y-4">[m
[32m+[m[32m        {paginatedData.map((item, index) => ([m
[32m+[m[32m          <div[m
[32m+[m[32m            key={index}[m
[32m+[m[32m            className="border rounded-xl shadow-sm p-4 bg-gray-50 text-gray-700"[m
[32m+[m[32m          >[m
[32m+[m[32m            {tbody.dataPropertys.map((property, i) => ([m
[32m+[m[32m              <p key={i} className="text-sm mb-1">[m
[32m+[m[32m                <span className="font-semibold">{thead.titles[i]}:</span>{" "}[m
[32m+[m[32m                {item[property] ?? "-"}[m
[32m+[m[32m              </p>[m
[32m+[m[32m            ))}[m
[32m+[m
[32m+[m[32m            {/* Estado */}[m
[32m+[m[32m            {tbody.state && ([m
[32m+[m[32m              <p className="mt-1 text-sm font-medium">[m
[32m+[m[32m                <span className="font-semibold">Estado: </span>[m
[32m+[m[32m                <span[m
[32m+[m[32m                  className={[m
[32m+[m[32m                    (item.Estado || "").toLowerCase() === "activo"[m
[32m+[m[32m                      ? "text-green-600"[m
[32m+[m[32m                      : "text-red-500"[m
[32m+[m[32m                  }[m
[32m+[m[32m                >[m
[32m+[m[32m                  {item.Estado ?? "Sin estado"}[m
[32m+[m[32m                </span>[m
[32m+[m[32m              </p>[m
[32m+[m[32m            )}[m
[32m+[m
[32m+[m[32m            {/* Acciones */}[m
[32m+[m[32m            <div className="flex items-center gap-3 mt-3 flex-wrap">[m
[32m+[m[32m              {onEdit && ([m
[32m+[m[32m                <button[m
[32m+[m[32m                  onClick={() => onEdit(item)}[m
[32m+[m[32m                  className="px-3 py-1 rounded-lg bg-blue-100 text-blue-600 hover:bg-blue-600 hover:text-white transition-colors text-xs"[m
[32m+[m[32m                >[m
[32m+[m[32m                  Editar[m
[32m+[m[32m                </button>[m
[32m+[m[32m              )}[m
[32m+[m[32m              {onDelete && ([m
[32m+[m[32m                <button[m
[32m+[m[32m                  onClick={() => onDelete(item)}[m
[32m+[m[32m                  className="px-3 py-1 rounded-lg bg-red-100 text-red-500 hover:bg-red-500 hover:text-white transition-colors text-xs"[m
[32m+[m[32m                >[m
[32m+[m[32m                  Eliminar[m
[32m+[m[32m                </button>[m
[32m+[m[32m              )}[m
[32m+[m[32m              {onView && ([m
[32m+[m[32m                <button[m
[32m+[m[32m                  onClick={() => onView(item)}[m
[32m+[m[32m                  className="px-3 py-1 rounded-lg bg-purple-100 text-purple-600 hover:bg-purple-600 hover:text-white transition-colors text-xs"[m
[32m+[m[32m                >[m
[32m+[m[32m                  Ver[m
[32m+[m[32m                </button>[m
[32m+[m[32m              )}[m
[32m+[m[32m              {onList && ([m
[32m+[m[32m                <button[m
[32m+[m[32m                  onClick={() => onList(item)}[m
[32m+[m[32m                  className="px-3 py-1 rounded-lg bg-gray-100 text-gray-600 hover:bg-gray-600 hover:text-white transition-colors text-xs"[m
[32m+[m[32m                >[m
[32m+[m[32m                  Lista Detallada[m
[32m+[m[32m                </button>[m
[32m+[m[32m              )}[m
[32m+[m[32m            </div>[m
[32m+[m[32m          </div>[m
[32m+[m[32m        ))}[m
[32m+[m[32m      </div>[m
[32m+[m
[32m+[m[32m      {/* ==== Paginaci√≥n ==== */}[m
[32m+[m[32m      {showPagination && ([m
[32m+[m[32m        <div className="w-full border-t border-gray-100 bg-gray-50">[m
[32m+[m[32m          <Pagination[m
[32m+[m[32m            currentPage={currentPage}[m
[32m+[m[32m            totalPages={totalPages}[m
[32m+[m[32m            onPageChange={handlePageChange}[m
[32m+[m[32m            totalRows={totalRows}[m
[32m+[m[32m            rowsPerPage={rowsPerPage}[m
[32m+[m[32m            startIndex={startIndex}[m
[32m+[m[32m          />[m
[32m+[m[32m        </div>[m
[32m+[m[32m      )}[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default Table;[m
