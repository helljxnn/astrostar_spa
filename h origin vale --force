[32m+[m[32mimport { useState, useMemo } from "react";[m
[32m+[m[32mimport Table from "./components/table/table";[m
[32m+[m[32mimport SearchInput from "../../../../../../../shared/components/SearchInput";[m
[32m+[m[32mimport SportsCategoryModal from "./components/SportsCategoryModal";[m
[32m+[m[32mimport SportsCategoryData from "../../../../../../../shared/models/sportsCategoryData";[m
