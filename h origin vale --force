[1mdiff --git a/src/shared/components/loaderContext.jsx b/src/shared/components/loaderContext.jsx[m
[1mnew file mode 100644[m
[1mindex 0000000..d2b03a3[m
[1m--- /dev/null[m
[1m+++ b/src/shared/components/loaderContext.jsx[m
[36m@@ -0,0 +1,29 @@[m
[32m+[m[32mimport React, { createContext, useState, useContext, useMemo } from 'react';[m
[32m+[m[32mimport Loader from '../components/loader';[m
[32m+[m
[32m+[m[32mconst LoaderContext = createContext(null);[m
[32m+[m
[32m+[m[32mexport const LoaderProvider = ({ children }) => {[m
[32m+[m[32m    const [isLoading, setIsLoading] = useState(false);[m
[32m+[m
[32m+[m[32m    // Usamos useMemo para optimizar y evitar que el valor del contexto cambie en cada render.[m
[32m+[m[32m    const contextValue = useMemo(() => ({[m
[32m+[m[32m        showLoading: () => setIsLoading(true),[m
[32m+[m[32m        hideLoading: () => setIsLoading(false),[m
[32m+[m[32m    }), []);[m
[32m+[m
[32m+[m[32m    return ([m
[32m+[m[32m        <LoaderContext.Provider value={contextValue}>[m
[32m+[m[32m            {children}[m
[32m+[m[32m            {isLoading && <Loader />}[m
[32m+[m[32m        </LoaderContext.Provider>[m
[32m+[m[32m    );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport const useLoading = () => {[m
[32m+[m[32m    const context = useContext(LoaderContext);[m
[32m+[m[32m    if (context === null) {[m
[32m+[m[32m        throw new Error('useLoading debe ser usado dentro de un LoaderProvider');[m
[32m+[m[32m    }[m
[32m+[m[32m    return context;[m
[32m+[m[32m};[m
