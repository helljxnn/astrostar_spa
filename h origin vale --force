[32m+[m[32mimport React, { useState, useEffect } from "react";[m
[32m+[m[32mimport Form from "../../../../../../../shared/components/form";[m
[32m+[m[32mimport HandleOnChange from "../../../../../../../shared/hooks/handleChange";[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Modal con el formulario para editar un material deportivo existente.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} props - Propiedades del componente.[m
[32m+[m[32m * @param {boolean} props.isOpen - Controla si el modal est√° visible.[m
[32m+[m[32m * @param {function} props.onClose - Funci√≥n para cerrar el modal.[m
[32m+[m[32m * @param {object} props.equipmentData - Los datos del material a editar.[m
[32m+[m[32m * @param {function} props.onSave - Funci√≥n que se ejecuta al guardar los cambios. Recibe los datos actualizados.[m
[32m+[m[32m */[m
[32m+[m[32mconst FormEdit = ({ isOpen, onClose, equipmentData, onSave }) => {[m
[32m+[m[32m    const [formData, setFormData] = useState({[m
[32m+[m[32m        nombre: "",[m
[32m+[m[32m        comprado: "",[m
[32m+[m[32m        donado: "",[m
[32m+[m[32m        estado: ""[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Cuando el modal se abre o los datos del equipo cambian, llenamos el formulario.[m
[32m+[m[32m    useEffect(() => {[m
[32m+[m[32m        if (equipmentData) {[m
[32m+[m[32m            setFormData({[m
[32m+[m[32m                nombre: equipmentData.NombreMaterial || "",[m
[32m+[m[32m                comprado: equipmentData.CantidadComprado || 0,[m
[32m+[m[32m                donado: equipmentData.CantidadDonado || 0,[m
[32m+[m[32m                estado: equipmentData.estado || ""[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }, [equipmentData]);[m
[32m+[m
[32m+[m[32m    const SaveData = (e) => {[m
[32m+[m[32m        HandleOnChange(formData, e, setFormData);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    const handleFormSubmit = (dataFromForm) => {[m
[32m+[m[32m        if (onSave) {[m
[32m+[m[32m            onSave(dataFromForm);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return ([m
[32m+[m[32m        <Form[m
[32m+[m[32m            isOpen={isOpen}[m
[32m+[m[32m            title="Editar Material"[m
[32m+[m[32m            submitText="Guardar Cambios"[m
[32m+[m[32m            onClose={onClose}[m
[32m+[m[32m            onSubmit={handleFormSubmit}[m
[32m+[m[32m            formData={formData}[m
[32m+[m[32m        >[m
[32m+[m[32m            {/* Estos son los 'children' del formulario */}[m
[32m+[m[32m            <div className="flex flex-col gap-4">[m
[32m+[m[32m                <div><label htmlFor="nombre" className="block text-sm font-medium text-gray-700 mb-1">Nombre</label><input type="text" id="nombre" name="nombre" value={formData.nombre} onChange={SaveData} className="w-full px-3 py-2 bg-white border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-primary-purple focus:border-primary-purple" placeholder="Ej: Bal√≥n de f√∫tbol" /></div>[m
[32m+[m[32m                <div><label htmlFor="comprado" className="block text-sm font-medium text-gray-700 mb-1">Cantidad Comprada</label><input type="number" id="comprado" name="comprado" value={formData.comprado} onChange={SaveData} className="w-full px-3 py-2 bg-white border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-primary-purple focus:border-primary-purple" placeholder="0" /></div>[m
[32m+[m[32m                <div><label htmlFor="donado" className="block text-sm font-medium text-gray-700 mb-1">Cantidad Donada</label><input type="number" id="donado" name="donado" value={formData.donado} onChange={SaveData} className="w-full px-3 py-2 bg-white border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-primary-purple focus:border-primary-purple" placeholder="0" /></div>[m
[32m+[m[32m                <div><label htmlFor="estado" className="block text-sm font-medium text-gray-700 mb-1">Estado</label><select id="estado" name="estado" value={formData.estado} onChange={SaveData} className="w-full px-3 py-2 bg-white border border-gray-300 rounded-md shadow-sm focus:outline-none focus:ring-primary-purple focus:border-primary-purple"><option value="" disabled>Seleccionar estado</option><option value="Activo">Activo</option><option value="Inactivo">Inactivo</option></select></div>[m
[32m+[m[32m            </div>[m
[32m+[m[32m        </Form>[m
[32m+[m[32m    );[m
[32m+[m[32m};[m
