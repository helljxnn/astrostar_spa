[1mdiff --git a/src/features/dashboard/pages/Admin/pages/Athletes/SportsCategory/hooks/useFormSportsCategoryValidation.js b/src/features/dashboard/pages/Admin/pages/Athletes/SportsCategory/hooks/useFormSportsCategoryValidation.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7ed7704[m
[1m--- /dev/null[m
[1m+++ b/src/features/dashboard/pages/Admin/pages/Athletes/SportsCategory/hooks/useFormSportsCategoryValidation.js[m
[36m@@ -0,0 +1,101 @@[m
[32m+[m[32mimport { useState } from "react";[m
[32m+[m
[32m+[m[32mexport const useFormSportsCategoryValidation = (initialValues = {}, validationRules = {}) => {[m
[32m+[m[32m  const [values, setValues] = useState(initialValues);[m
[32m+[m[32m  const [errors, setErrors] = useState({});[m
[32m+[m[32m  const [touched, setTouched] = useState({});[m
[32m+[m
[32m+[m[32m  const validateField = (name, value, currentValues = values) => {[m
[32m+[m[32m    const rules = validationRules[name];[m
[32m+[m[32m    if (!rules) return "";[m
[32m+[m[32m    for (const rule of rules) {[m
[32m+[m[32m      const error = rule(value, currentValues);[m
[32m+[m[32m      if (error) return error;[m
[32m+[m[32m    }[m
[32m+[m[32m    return "";[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const validateAllFields = (vals = values) => {[m
[32m+[m[32m    const newErrors = {};[m
[32m+[m[32m    Object.keys(validationRules).forEach((name) => {[m
[32m+[m[32m      const error = validateField(name, vals[name], vals);[m
[32m+[m[32m      if (error) newErrors[name] = error;[m
[32m+[m[32m    });[m
[32m+[m[32m    setErrors(newErrors);[m
[32m+[m[32m    return Object.keys(newErrors).length === 0;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ Funci√≥n validate para compatibilidad con el modal[m
[32m+[m[32m  const validate = (formData) => {[m
[32m+[m[32m    return validateAllFields(formData);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ para marcar todos los campos como "tocados"[m
[32m+[m[32m  const touchAllFields = (vals = values) => {[m
[32m+[m[32m    const allTouched = {};[m
[32m+[m[32m    Object.keys(vals).forEach((k) => { allTouched[k] = true; });[m
[32m+[m[32m    setTouched(allTouched);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleChange = (name, value) => {[m
[32m+[m[32m    setValues((prev) => ({ ...prev, [name]: value }));[m
[32m+[m[32m    if (touched[name]) {[m
[32m+[m[32m      const error = validateField(name, value, { ...values, [name]: value });[m
[32m+[m[32m      setErrors((prev) => ({ ...prev, [name]: error }));[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ‚úÖ handleBlur compatible con el modal (recibe name, value, form)[m
[32m+[m[32m  const handleBlur = (name, value = values[name], formData = values) => {[m
[32m+[m[32m    setTouched((prev) => ({ ...prev, [name]: true }));[m
[32m+[m[32m    const error = validateField(name, value, formData);[m
[32m+[m[32m    setErrors((prev) => ({ ...prev, [name]: error }));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    values,[m
[32m+[m[32m    errors,[m
[32m+[m[32m    touched,[m
[32m+[m[32m    handleChange,[m
[32m+[m[32m    handleBlur,[m
[32m+[m[32m    validateAllFields,[m
[32m+[m[32m    validate, // ‚úÖ Agregado para compatibilidad[m
[32m+[m[32m    touchAllFields,[m
[32m+[m[32m    setValues,[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// ‚úÖ Reglas de validaci√≥n actualizadas y m√°s espec√≠ficas[m
[32m+[m[32mexport const sportsCategoryValidationRules = {[m
[32m+[m[32m  nombreCategoria: [[m
[32m+[m[32m    (v) => (!v?.trim() ? "El nombre de la categor√≠a es obligatorio" : ""),[m
[32m+[m[32m    (v) => (v?.trim().length < 3 ? "El nombre debe tener al menos 3 caracteres" : ""),[m
[32m+[m[32m    (v) => (v?.trim().length > 50 ? "El nombre no puede exceder 50 caracteres" : ""),[m
[32m+[m[32m  ],[m
[32m+[m[32m  edadMinima: [[m
[32m+[m[32m    (v) => (!v || v === "" ? "La edad m√≠nima es obligatoria" : ""),[m
[32m+[m[32m    (v) => (isNaN(Number(v)) ? "La edad m√≠nima debe ser un n√∫mero v√°lido" : ""),[m
[32m+[m[32m    (v) => (Number(v) < 5 ? "La edad m√≠nima debe ser mayor o igual a 5 a√±os" : ""),[m
[32m+[m[32m    (v) => (Number(v) > 50 ? "La edad m√≠nima no puede ser mayor a 50 a√±os" : ""),[m
[32m+[m[32m  ],[m
[32m+[m[32m  edadMaxima: [[m
[32m+[m[32m    (v) => (!v || v === "" ? "La edad m√°xima es obligatoria" : ""),[m
[32m+[m[32m    (v) => (isNaN(Number(v)) ? "La edad m√°xima debe ser un n√∫mero v√°lido" : ""),[m
[32m+[m[32m    (v) => (Number(v) > 80 ? "La edad m√°xima no puede ser mayor a 80 a√±os" : ""),[m
[32m+[m[32m    (v, values) =>[m
[32m+[m[32m      values?.edadMinima && Number(v) <= Number(values.edadMinima)[m
[32m+[m[32m        ? "La edad m√°xima debe ser mayor que la edad m√≠nima"[m
[32m+[m[32m        : "",[m
[32m+[m[32m  ],[m
[32m+[m[32m  descripcion: [[m
[32m+[m[32m    // Descripci√≥n opcional, pero si se llena debe tener m√≠nimo[m
[32m+[m[32m    (v) => (v && v.trim().length > 0 && v.trim().length < 10 ? "La descripci√≥n debe tener al menos 10 caracteres" : ""),[m
[32m+[m[32m    (v) => (v && v.trim().length > 500 ? "La descripci√≥n no puede exceder 500 caracteres" : ""),[m
[32m+[m[32m  ],[m
[32m+[m[32m  estado: [[m
[32m+[m[32m    (v) => (!v ? "Debe seleccionar un estado para la categor√≠a" : ""),[m
[32m+[m[32m  ],[m
[32m+[m[32m  archivo: [[m
[32m+[m[32m    (v) => (!v ? "Debe subir un archivo antes de continuar" : ""),[m
[32m+[m[32m  ],[m
[32m+[m[32m};[m
