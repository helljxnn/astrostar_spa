[32m+[m[32mimport React, { useState } from "react";[m
[32m+[m[32mimport { FaPlus } from "react-icons/fa";[m
[32m+[m[32mimport EmployeesScheduleCalendar from "./components/EmployeesScheduleCalendar";[m
[32m+[m[32mimport ScheduleModal from "./components/EmployeesScheduleModal";[m
[32m+[m[32mimport { format } from "date-fns";[m
[32m+[m
[32m+[m[32mconst EmployeeSchedule = () => {[m
[32m+[m[32m  const [schedules, setSchedules] = useState([]); // eventos del calendario[m
[32m+[m[32m  const [isModalOpen, setIsModalOpen] = useState(false);[m
[32m+[m[32m  const [selectedSchedule, setSelectedSchedule] = useState(null);[m
[32m+[m[32m  const [isNew, setIsNew] = useState(true);[m
[32m+[m
[32m+[m[32m  // Abrir modal para crear un nuevo horario (recibe start/end como Date desde el calendario)[m
[32m+[m[32m  const openModalForSlot = ({ start, end }) => {[m
[32m+[m[32m    const payload = {[m
[32m+[m[32m      empleado: "",[m
[32m+[m[32m      fecha: format(start, "yyyy-MM-dd"),[m
[32m+[m[32m      horaInicio: format(start, "HH:mm"),[m
[32m+[m[32m      horaFin: format(end, "HH:mm"),[m
[32m+[m[32m      area: "",[m
[32m+[m[32m      estado: "Programado",[m
[32m+[m[32m      observaciones: "",[m
[32m+[m[32m      id: null,[m
[32m+[m[32m    };[m
[32m+[m[32m    setSelectedSchedule(payload);[m
[32m+[m[32m    setIsNew(true);[m
[32m+[m[32m    setIsModalOpen(true);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Abrir modal para editar un horario (recibe el evento del calendario)[m
[32m+[m[32m  const openModalForEvent = (event) => {[m
[32m+[m[32m    const payload = {[m
[32m+[m[32m      empleado: event.title?.replace("Turno - ", "") || "",[m
[32m+[m[32m      fecha: format(event.start, "yyyy-MM-dd"),[m
[32m+[m[32m      horaInicio: format(event.start, "HH:mm"),[m
[32m+[m[32m      horaFin: format(event.end, "HH:mm"),[m
[32m+[m[32m      area: event.area || "",[m
[32m+[m[32m      estado: event.estado || "Programado",[m
[32m+[m[32m      observaciones: event.observaciones || "",[m
[32m+[m[32m      id: event.id,[m
[32m+[m[32m    };[m
[32m+[m[32m    setSelectedSchedule(payload);[m
[32m+[m[32m    setIsNew(false);[m
[32m+[m[32m    setIsModalOpen(true);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Guardar (veniendo del modal)[m
[32m+[m[32m  const handleSaveFromModal = (form) => {[m
[32m+[m[32m    // Conversi√≥n a objeto que react-big-calendar entiende[m
[32m+[m[32m    const start = new Date(`${form.fecha}T${form.horaInicio}`);[m
[32m+[m[32m    const end = new Date(`${form.fecha}T${form.horaFin}`);[m
[32m+[m
[32m+[m[32m    const eventObj = {[m
[32m+[m[32m      id: form.id || Date.now(),[m
[32m+[m[32m      title: `Turno - ${form.empleado}`,[m
[32m+[m[32m      start,[m
[32m+[m[32m      end,[m
[32m+[m[32m      color: "bg-primary-purple",[m
[32m+[m[32m      empleado: form.empleado,[m
[32m+[m[32m      area: form.area,[m
[32m+[m[32m      estado: form.estado,[m
[32m+[m[32m      observaciones: form.observaciones,[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (isNew) {[m
[32m+[m[32m      setSchedules((prev) => [...prev, eventObj]);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      setSchedules((prev) => prev.map((s) => (s.id === eventObj.id ? eventObj : s)));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    setIsModalOpen(false);[m
[32m+[m[32m    setSelectedSchedule(null);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="font-monserrat p-6">[m
[32m+[m[32m      <div className="flex justify-between items-center mb-6">[m
[32m+[m[32m        <h1 className="text-2xl font-semibold text-gray-800">Horario de Empleados</h1>[m
[32m+[m[32m        <button[m
[32m+[m[32m          onClick={() =>[m
[32m+[m[32m            openModalForSlot({[m
[32m+[m[32m              start: new Date(), // si quieres abrir el modal vac√≠o con hoy; o puedes usar un slot por defecto[m
[32m+[m[32m              end: new Date(new Date().getTime() + 60 * 60 * 1000),[m
[32m+[m[32m            })[m
[32m+[m[32m          }[m
[32m+[m[32m          className="flex items-center gap-2 px-4 py-2 bg-gradient-to-r from-primary-purple to-primary-blue text-white rounded-lg shadow hover:opacity-90 transition"[m
[32m+[m[32m        >[m
[32m+[m[32m          <FaPlus /> Nuevo Horario[m
[32m+[m[32m        </button>[m
[32m+[m[32m      </div>[m
[32m+[m
[32m+[m[32m      <div className="mt-2">[m
[32m+[m[32m        <EmployeesScheduleCalendar[m
[32m+[m[32m          schedules={schedules}[m
[32m+[m[32m          onOpenModalForSlot={openModalForSlot}[m
[32m+[m[32m          onOpenModalForEvent={openModalForEvent}[m
[32m+[m[32m        />[m
[32m+[m[32m      </div>[m
[32m+[m
[32m+[m[32m      {isModalOpen && ([m
[32m+[m[32m        <ScheduleModal[m
[32m+[m[32m          schedule={selectedSchedule}[m
[32m+[m[32m          isNew={isNew}[m
[32m+[m[32m          onClose={() => setIsModalOpen(false)}[m
[32m+[m[32m          onSave={handleSaveFromModal}[m
[32m+[m[32m        />[m
[32m+[m[32m      )}[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default EmployeeSchedule;[m
