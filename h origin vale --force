[32m+[m[32m  apellido: [[m[41m[m
[32m+[m[32m    (value) => (!value?.trim() ? "El apellido es obligatorio" : ""),[m[41m[m
[32m+[m[32m    (value) => value?.length < 3 ? "El apellido debe tener al menos 3 caracteres" : "",[m[41m[m
[32m+[m[32m    (value) =>[m[41m[m
[32m+[m[32m      !/^[a-zA-Z√Å√â√ç√ì√ö√°√©√≠√≥√∫√±√ë\s]+$/.test(value || "") ? "Solo se permiten letras" : "",[m[41m[m
[32m+[m[32m  ],[m[41m[m
[32m+[m[32m  correo: [[m[41m[m
[32m+[m[32m    (value) => (!value?.trim() ? "El correo es obligatorio" : ""),[m[41m[m
[32m+[m[32m    (value) =>[m[41m[m
[32m+[m[32m      !/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(value || "") ? "Formato de correo inv√°lido" : "",[m[41m[m
[32m+[m[32m  ],[m[41m[m
[32m+[m[32m  telefono: [[m[41m[m
[32m+[m[32m    (value) => (!value?.trim() ? "El tel√©fono es obligatorio" : ""),[m[41m[m
[32m+[m[32m    (value) => !/^\d+$/.test(value || "") ? "Solo se permiten n√∫meros" : "",[m[41m[m
[32m+[m[32m    (value) =>[m[41m[m
[32m+[m[32m      value?.length < 7 || value?.length > 15 ? "El tel√©fono debe tener entre 7 y 15 d√≠gitos" : "",[m[41m[m
[32m+[m[32m  ],[m[41m[m
[32m+[m[32m  edad: [[m[41m[m
[32m+[m[32m    (value) => (!value ? "La edad es obligatoria" : ""),[m[41m[m
[32m+[m[32m    (value) => !/^\d+$/.test(value || "") ? "La edad debe ser un n√∫mero" : "",[m[41m[m
[32m+[m[32m    (value) => parseInt(value) < 18 ? "Debe ser mayor de 18 a√±os" : "",[m[41m[m
[32m+[m[32m    (value) => parseInt(value) > 65 ? "La edad m√°xima permitida es 65 a√±os" : "",[m[41m[m
[32m+[m[32m  ],[m[41m[m
[32m+[m[32m  identificacion: [[m[41m[m
[32m+[m[32m    (value) => (!value?.trim() ? "La identificaci√≥n es obligatoria" : ""),[m[41m[m
[32m+[m[32m    (value) => !/^\d+$/.test(value || "") ? "Solo se permiten n√∫meros" : "",[m[41m[m
[32m+[m[32m  ],[m[41m[m
[32m+[m[32m  tipoDocumento: [(value) => (!value ? "Debe seleccionar el tipo de documento" : "")],[m[41m[m
[32m+[m[32m  tipoEmpleado: [(value) => (!value ? "Debe seleccionar el tipo de empleado" : "")],[m[41m[m
[32m+[m[32m  rol: [(value) => (!value ? "Debe seleccionar un rol" : "")],[m[41m[m
