[32m+[m[32m            {hasActions && ([m
[32m+[m[32m              <td className="px-6 py-4 flex items-center justify-center gap-2">[m
[32m+[m[32m                {onView && ([m
[32m+[m[32m                  <button[m
[32m+[m[32m                    onClick={() => onView(item)}[m
[32m+[m[32m                    className="p-2 rounded-full bg-purple-100 text-purple-600 hover:bg-purple-600 hover:text-white transition"[m
[32m+[m[32m                    title="Ver Detalle"[m
[32m+[m[32m                  >[m
[32m+[m[32m                    <FaEye />[m
[32m+[m[32m                  </button>[m
[32m+[m[32m                )}[m
[32m+[m[32m                {onList && ([m
[32m+[m[32m                  <button[m
[32m+[m[32m                    onClick={() => onList(item)}[m
[32m+[m[32m                    className="p-2 rounded-full bg-gray-100 text-gray-600 hover:bg-gray-600 hover:text-white transition"[m
[32m+[m[32m                    title="Ver Lista"[m
[32m+[m[32m                  >[m
[32m+[m[32m                    <FaList />[m
[32m+[m[32m                  </button>[m
[32m+[m[32m                )}[m
[32m+[m[32m                {onEdit && ([m
[32m+[m[32m                  <button[m
[32m+[m[32m                    onClick={() => onEdit(item)}[m
[32m+[m[32m                    className="p-2 rounded-full bg-blue-100 text-blue-600 hover:bg-blue-600 hover:text-white transition"[m
[32m+[m[32m                    title="Editar"[m
[32m+[m[32m                  >[m
[32m+[m[32m                    <FaRegEdit />[m
[32m+[m[32m                  </button>[m
[32m+[m[32m                )}[m
[32m+[m[32m                {onDelete && ([m
[32m+[m[32m                  <button[m
[32m+[m[32m                    onClick={() => onDelete(item)}[m
[32m+[m[32m                    className="p-2 rounded-full bg-red-100 text-red-500 hover:bg-red-500 hover:text-white transition"[m
[32m+[m[32m                    title="Eliminar"[m
[32m+[m[32m                  >[m
[32m+[m[32m                    <FaTrash />[m
[32m+[m[32m                  </button>[m
[32m+[m[32m                )}[m
[32m+[m[32m              </td>[m
[32m+[m[32m            )}[m
[32m+[m[32m          </motion.tr>[m
[32m+[m[32m        );[m
[32m+[m[32m      })}[m
