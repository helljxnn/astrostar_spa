[36m@@ -0,0 +1,104 @@[m
[32m+[m[32m// useFormEventValidation.js[m[41m[m
[32m+[m[32mimport { useState } from "react";[m[41m[m
[32m+[m[41m[m
[32m+[m[32mexport const useFormEventValidation = () => {[m[41m[m
[32m+[m[32m  const [errors, setErrors] = useState({});[m[41m[m
[32m+[m[32m  const [touched, setTouched] = useState({});[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const validateField = (name, value, formData) => {[m[41m[m
[32m+[m[32m    let error = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    switch (name) {[m[41m[m
[32m+[m[32m      case "nombre":[m[41m[m
[32m+[m[32m        if (!value?.trim()) error = "El nombre del evento es obligatorio.";[m[41m[m
[32m+[m[32m        else if (value.length < 3) error = "El nombre debe tener al menos 3 caracteres.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "descripcion":[m[41m[m
[32m+[m[32m        if (!value?.trim()) error = "La descripci√≥n es obligatoria.";[m[41m[m
[32m+[m[32m        else if (value.length < 10) error = "La descripci√≥n debe tener al menos 10 caracteres.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "fechaInicio":[m[41m[m
[32m+[m[32m        if (!value) error = "La fecha de inicio es obligatoria.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "fechaFin":[m[41m[m
[32m+[m[32m        if (!value) error = "La fecha de finalizaci√≥n es obligatoria.";[m[41m[m
[32m+[m[32m        else if (formData.fechaInicio && value < formData.fechaInicio) {[m[41m[m
[32m+[m[32m          error = "La fecha de finalizaci√≥n no puede ser menor a la de inicio.";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "ubicacion":[m[41m[m
[32m+[m[32m        if (!value?.trim()) error = "La ubicaci√≥n es obligatoria.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "telefono":[m[41m[m
[32m+[m[32m        if (!value?.trim()) error = "El tel√©fono es obligatorio.";[m[41m[m
[32m+[m[32m        else if (!/^\d{7,15}$/.test(value)) {[m[41m[m
[32m+[m[32m          error = "Ingrese un n√∫mero v√°lido (7 a 15 d√≠gitos).";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "imagen":[m[41m[m
[32m+[m[32m        if (!value) error = "Debe subir una imagen.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "detalles":[m[41m[m
[32m+[m[32m        if (!value?.trim()) error = "Los detalles son obligatorios.";[m[41m[m
[32m+[m[32m        else if (value.length < 15) error = "Los detalles deben tener al menos 15 caracteres.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "patrocinador":[m[41m[m
[32m+[m[32m        if (!Array.isArray(value) || value.length === 0) {[m[41m[m
[32m+[m[32m          error = "Seleccione al menos un patrocinador.";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "categoria":[m[41m[m
[32m+[m[32m        if (!value?.trim()) error = "Debe seleccionar una categor√≠a.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "estado":[m[41m[m
[32m+[m[32m        if (!value?.trim()) error = "Debe seleccionar un estado.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      case "tipoEvento":[m[41m[m
[32m+[m[32m        if (!value?.trim()) error = "Debe seleccionar un tipo de evento.";[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      default:[m[41m[m
[32m+[m[32m        break;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    return error;[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const validate = (formData) => {[m[41m[m
[32m+[m[32m    const newErrors = {};[m[41m[m
[32m+[m[32m    Object.keys(formData).forEach((key) => {[m[41m[m
[32m+[m[32m      const error = validateField(key, formData[key], formData);[m[41m[m
[32m+[m[32m      if (error) newErrors[key] = error;[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[32m    setErrors(newErrors);[m[41m[m
[32m+[m[32m    return Object.keys(newErrors).length === 0;[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const handleBlur = (name, value, formData) => {[m[41m[m
[32m+[m[32m    setTouched((prev) => ({ ...prev, [name]: true }));[m[41m[m
[32m+[m[32m    const error = validateField(name, value, formData);[m[41m[m
[32m+[m[32m    setErrors((prev) => ({ ...prev, [name]: error }));[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const touchAllFields = (formData) => {[m[41m[m
[32m+[m[32m    const allTouched = {};[m[41m[m
[32m+[m[32m    Object.keys(formData).forEach((name) => {[m[41m[m
[32m+[m[32m      allTouched[name] = true;[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[32m    allTouched.tipoEvento = true;[m[41m[m
[32m+[m[32m    setTouched(allTouched);[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return { errors, touched, validate, handleBlur, touchAllFields };[m[41m[m
[32m+[m[32m};[m[41m[m
[1mdiff --git a/src/features/dashboard/pages/Admin/pages/Purchases/Providers/components/ProviderModal.jsx b/src/features/dashboard/pages/Admin/pages/Purchases/Providers/components/ProviderModal.jsx[m
[1mnew file mode 100644[m
[1mindex 0000000..89cfa8c[m
[1m--- /dev/null[m
[1m+++ b/src/features/dashboard/pages/Admin/pages/Purchases/Providers/components/ProviderModal.jsx[m
