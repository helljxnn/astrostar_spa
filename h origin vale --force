[32m+[m[32mimport React from "react";[m[41m[m
[32m+[m[32mimport Table from "../../../../../../shared/components/Table/table";[m[41m[m
[32m+[m[41m[m
[32m+[m[32mexport const RolesTable = ({ roles, onEdit, onDelete, onView }) => {[m[41m[m
[32m+[m[32m  return ([m[41m[m
[32m+[m[32m    <div className="mt-8 bg-white shadow-lg rounded-2xl p-6">[m[41m[m
[32m+[m[32m      <h3 className="text-xl font-bold text-[#9BE9FF] mb-4">Lista de Roles</h3>[m[41m[m
