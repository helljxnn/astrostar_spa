[32m+[m[32mimport React, { useEffect } from "react";[m[41m[m
[32m+[m[32mimport { motion } from "framer-motion";[m[41m[m
[32m+[m[32mimport { FormField } from "../../../../../../../../shared/components/FormField";[m[41m[m
[32m+[m[32mimport {[m[41m[m
[32m+[m[32m  useFormEmployeeValidation,[m[41m[m
[32m+[m[32m  employeeValidationRules,[m[41m[m
[32m+[m[32m} from "../hooks/useFormEmployeeValidation";[m[41m[m
