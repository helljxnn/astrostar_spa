[32m+[m[32mimport React from "react";[m
[32m+[m
[32m+[m[32mconst HandleOnChange = (formData , e, nameFunction) => {[m
[32m+[m[32m  const { name, value } = e.target;[m
[32m+[m[32m  nameFunction({[m
[32m+[m[32m    ...formData,[m
[32m+[m[32m    [name]: value,[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default HandleOnChange;[m
