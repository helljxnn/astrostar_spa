[32m+[m[32mimport React from "react";[m
[32m+[m[32mimport { motion } from "framer-motion";[m
[32m+[m[32mimport { FaEye, FaRegEdit, FaTrash, FaList } from "react-icons/fa";[m
[32m+[m
[32m+[m[32m/* --- Animaci√≥n de filas --- */[m
[32m+[m[32mconst rowVariants = {[m
[32m+[m[32m  hidden: { opacity: 0, y: 10 },[m
[32m+[m[32m  visible: (i) => ({[m
[32m+[m[32m    opacity: 1,[m
[32m+[m[32m    y: 0,[m
[32m+[m[32m    transition: { delay: i * 0.05, duration: 0.4, ease: "easeOut" },[m
[32m+[m[32m  }),[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst Tbody = ({[m
[32m+[m[32m  data = [],[m
[32m+[m[32m  dataPropertys = [],[m
[32m+[m[32m  state = false,[m
[32m+[m[32m  onEdit,[m
[32m+[m[32m  onDelete,[m
[32m+[m[32m  onView,[m
[32m+[m[32m  onList,[m
[32m+[m[32m}) => {[m
[32m+[m[32m  const hasActions = onEdit || onDelete || onView || onList;[m
[32m+[m
[32m+[m[32m  /* üîπ Sin datos */[m
[32m+[m[32m  if (!data || data.length === 0) {[m
[32m+[m[32m    return ([m
[32m+[m[32m      <tbody>[m
[32m+[m[32m        <tr>[m
[32m+[m[32m          <td colSpan="100%" className="p-6 text-center text-gray-400 italic">[m
[32m+[m[32m            No hay datos para mostrar.[m
[32m+[m[32m          </td>[m
[32m+[m[32m        </tr>[m
[32m+[m[32m      </tbody>[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <tbody id="tbody" className="divide-y divide-gray-200">[m
[32m+[m[32m      {data.map((item, index) => {[m
[32m+[m[32m        // Estado seguro en string[m
[32m+[m[32m        const estadoOriginal = item.Estado ?? item.estado ?? "";[m
[32m+[m[32m        const estado = String(estadoOriginal).toLowerCase();[m
[32m+[m
[32m+[m[32m        /* üé® Colores de estado */[m
[32m+[m[32m        let estadoClass =[m
[32m+[m[32m          "px-3 py-1 rounded-full text-sm font-medium border w-fit";[m
[32m+[m[32m        if (estado === "activo") {[m
[32m+[m[32m          estadoClass += " text-green-700 bg-green-100 border-green-300";[m
[32m+[m[32m        } else if (estado === "inactivo") {[m
[32m+[m[32m          estadoClass += " text-red-700 bg-red-100 border-red-300";[m
[32m+[m[32m        } else {[m
[32m+[m[32m          estadoClass += " text-gray-600 bg-gray-100 border-gray-300";[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return ([m
[32m+[m[32m          <motion.tr[m
[32m+[m[32m            key={index}[m
[32m+[m[32m            variants={rowVariants}[m
[32m+[m[32m            initial="hidden"[m
[32m+[m[32m            animate="visible"[m
[32m+[m[32m            custom={index}[m
[32m+[m[32m            className={`${[m
[32m+[m[32m              index % 2 === 0 ? "bg-white" : "bg-purple-50"[m
[32m+[m[32m            } hover:bg-purple-100 transition`}[m
[32m+[m[32m          >[m
[32m+[m[32m            {/* üîπ Columnas din√°micas */}[m
[32m+[m[32m            {dataPropertys.map((property, i) => ([m
[32m+[m[32m              <td[m
[32m+[m[32m                key={i}[m
[32m+[m[32m                className="px-6 py-4 whitespace-nowrap text-gray-700 text-sm"[m
[32m+[m[32m              >[m
[32m+[m[32m                {item[property] ?? "-"}[m
[32m+[m[32m              </td>[m
[32m+[m[32m            ))}[m
[32m+[m
[32m+[m[32m            {/* üîπ Estado */}[m
[32m+[m[32m            {state && ([m
[32m+[m[32m              <td className="px-6 py-4 whitespace-nowrap">[m
[32m+[m[32m                <span className={estadoClass}>{estadoOriginal}</span>[m
[32m+[m[32m              </td>[m
[32m+[m[32m            )}[m
[32m+[m
