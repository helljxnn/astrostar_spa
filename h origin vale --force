[32m+[m[32mimport React from "react";[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Componente de formulario din√°mico y reutilizable, ideal para modales.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} props - Propiedades del componente.[m
[32m+[m[32m * @param {boolean} props.isOpen - Controla si el modal est√° visible.[m
[32m+[m[32m * @param {string} props.title - El t√≠tulo que se mostrar√° en la cabecera del formulario.[m
[32m+[m[32m * @param {React.ReactNode} props.children - Los campos del formulario (inputs, selects, etc.) que se renderizar√°n en el cuerpo.[m
[32m+[m[32m * @param {function} props.onClose - Funci√≥n que se ejecuta al hacer clic en el bot√≥n "Cancelar".[m
[32m+[m[32m * @param {function} props.onSubmit - Funci√≥n que se ejecuta al enviar el formulario.[m
[32m+[m[32m * @param {string} props.submitText - El texto para el bot√≥n de env√≠o (ej. "Crear", "Guardar Cambios").[m
[32m+[m[32m * @param {number} props.id - El id de un registro si para actualizar o no.[m[41m  [m
[32m+[m[32m * @param {formData} props.formData - datos del formulario para la funcion[m
[32m+[m[32m*/[m
[32m+[m[32mconst Form = ({ isOpen, title, children, onClose, onSubmit, submitText = "Guardar", id = null, formData }) => {[m
[32m+[m
[32m+[m[32m    const handleSubmit = (e) => {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m        if (onSubmit) {[m
[32m+[m[32m            onSubmit(formData);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return ([m
[32m+[m[32m        <div[m
[32m+[m[32m            className={`fixed inset-0 z-50 transition-opacity duration-300 ease-in-out ${isOpen ? "opacity-100 pointer-events-auto" : "opacity-0 pointer-events-none"[m
[32m+[m[32m                }`}[m
[32m+[m[32m        >[m
[32m+[m[32m            {/* Backdrop: Fondo oscuro. No tiene onClick para evitar que se cierre desde afuera. */}[m
[32m+[m[32m            <div className="absolute inset-0 bg-black bg-opacity-60">[m
[32m+[m
[32m+[m[32m                {/* Contenedor del Formulario con animaci√≥n */}[m
[32m+[m[32m                <div[m
[32m+[m[32m                    className={`w-full h-full flex items-center justify-center transform transition-all duration-300 ease-in-out ${isOpen ? "scale-100 opacity-100" : "scale-95 opacity-0"}`}[m
[32m+[m[32m                >[m
[32m+[m[32m                    <form onSubmit={handleSubmit} className="bg-white rounded-lg shadow-xl p-8 w-2/5  flex flex-col gap-6">[m
[32m+[m[32m                        {/* Header */}[m
[32m+[m[32m                        <div className="border-b border-gray-200 pb-4 w-full">[m
[32m+[m[32m                            <h2 className="text-2xl font-questrial text-primary-purple">{title}</h2>[m
[32m+[m[32m                        </div>[m
[32m+[m
[32m+[m[32m                        {/* Body: Contenido din√°mico que se pasa como children */}[m
[32m+[m[32m                        <div className="space-y-4 w-full">[m
[32m+[m[32m                            {children}[m
[32m+[m
[32m+[m[32m                            {id ? ([m
[32m+[m[32m                                <input type="hidden" name="id" value={id} />[m
[32m+[m[32m                            ): null}[m
[32m+[m[32m                        </div>[m
[32m+[m
[32m+[m[32m                        {/* Footer: Botones de acci√≥n */}[m
[32m+[m[32m                        <div className="flex justify-end gap-4 pt-4 border-t border-gray-200 w-full">[m
[32m+[m[32m                            <button type="button" onClick={onClose} className="px-6 py-2 rounded-lg bg-gray-100 text-gray-800 font-semibold hover:bg-gray-200 transition-colors">[m
[32m+[m[32m                                Cancelar[m
[32m+[m[32m                            </button>[m
[32m+[m[32m                            <button type="submit" className="px-6 py-2 rounded-lg bg-gradient-to-r from-primary-blue to-primary-purple text-white font-bold hover:opacity-90 transition-opacity">[m
[32m+[m[32m                                {submitText}[m
[32m+[m[32m                            </button>[m
[32m+[m[32m                        </div>[m
[32m+[m[32m                    </form>[m
[32m+[m[32m                </div>[m
[32m+[m[32m            </div>[m
[32m+[m[32m        </div>[m
[32m+[m[32m    )[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default Form;[m
