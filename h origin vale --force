[32m+[m[32m      className="text-gray-700 text-sm uppercase tracking-wider bg-gradient-to-r from-primary-purple to-primary-blue"[m
[32m+[m[32m    >[m
[32m+[m[32m      <tr>[m
[32m+[m[32m        {titles.map((title, index) => ([m
[32m+[m[32m          <th[m
[32m+[m[32m            key={index}[m
[32m+[m[32m            className="px-6 py-4 text-left font-semibold text-white"[m
[32m+[m[32m          >[m
[32m+[m[32m            {title}[m
[32m+[m[32m          </th>[m
[32m+[m[32m        ))}[m
