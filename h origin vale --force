[32m+[m[32m      tipoEmpleado: "",[m[41m[m
[32m+[m[32m      rol: "",[m[41m[m
[32m+[m[32m      estado: "",[m[41m[m
[32m+[m[32m      fechaAsignacion: "",[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    employeeValidationRules[m[41m[m
[32m+[m[32m  );[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Cargar datos si es edici√≥n[m[41m[m
[32m+[m[32m  useEffect(() => {[m[41m[m
[32m+[m[32m    if (employee) {[m[41m[m
[32m+[m[32m      setFormData(employee);[m[41m[m
[32m+[m[32m    } else {[m[41m[m
[32m+[m[32m      setFormData({[m[41m[m
[32m+[m[32m        nombre: "",[m[41m[m
