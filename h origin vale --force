[32m+[m[32mimport React, { useState, useEffect } from "react";[m
[32m+[m[32mimport { motion } from "framer-motion";[m
[32m+[m[32mimport { showSuccessAlert } from "../../../../../../../../shared/utils/alerts";[m
[32m+[m[32mimport {[m
[32m+[m[32m  useFormSportsCategoryValidation,[m
[32m+[m[32m  sportsCategoryValidationRules,[m
[32m+[m[32m} from "../hooks/useFormSportsCategoryValidation";[m
[32m+[m
[32m+[m[32mconst SportsCategoryModal = ({ isOpen, onClose, onSave, category, isNew = true }) => {[m
[32m+[m[32m  // Estado inicial del formulario[m
[32m+[m[32m  const initialForm = {[m
[32m+[m[32m    nombreCategoria: "",[m
[32m+[m[32m    edadMinima: "",[m
[32m+[m[32m    edadMaxima: "",[m
[32m+[m[32m    descripcion: "",[m
[32m+[m[32m    archivo: null,[m
[32m+[m[32m    estado: "",[m
[32m+[m[32m    publicar: false,[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const {[m
[32m+[m[32m    values,[m
[32m+[m[32m    errors,[m
[32m+[m[32m    touched,[m
[32m+[m[32m    handleChange,[m
[32m+[m[32m    handleBlur,[m
[32m+[m[32m    validateAllFields,[m
[32m+[m[32m    touchAllFields,[m
[32m+[m[32m    setValues,[m
[32m+[m[32m  } = useFormSportsCategoryValidation(initialForm, sportsCategoryValidationRules);[m
[32m+[m
[32m+[m[32m  // Prellenar solo si estamos editando[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    if (!isNew && category) {[m
[32m+[m[32m      const editForm = {[m
[32m+[m[32m        nombreCategoria: category.Nombre || category.nombreCategoria || "",[m
[32m+[m[32m        edadMinima: category.EdadMinima || category.edadMinima || "",[m
[32m+[m[32m        edadMaxima: category.EdadMaxima || category.edadMaxima || "",[m
[32m+[m[32m        descripcion: category.Descripcion || category.descripcion || "",[m
[32m+[m[32m        archivo: category.Archivo || category.archivo || null,[m
[32m+[m[32m        estado: category.Estado || category.estado || "",[m
[32m+[m[32m        publicar: category.Publicar || category.publicar || false,[m
[32m+[m[32m      };[m
[32m+[m[32m      setValues(editForm);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // Resetear formulario para nuevo registro[m
[32m+[m[32m      setValues(initialForm);[m
[32m+[m[32m    }[m
[32m+[m[32m  }, [category, isNew, isOpen, setValues]);[m
[32m+[m
[32m+[m[32m  // Manejar archivo[m
[32m+[m[32m  const handleFileChange = (e) => {[m
[32m+[m[32m    const file = e.target.files[0];[m
[32m+[m[32m    handleChange("archivo", file);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Env√≠o del formulario[m
[32m+[m[32m  const handleSubmit = (e) => {[m
[32m+[m[32m    e && e.preventDefault();[m
[32m+[m[41m    [m
[32m+[m[32m    console.log("Valores actuales del formulario:", values);[m
[32m+[m[41m    [m
[32m+[m[32m    // Marcar todos los campos como tocados y validar[m
[32m+[m[32m    touchAllFields(values);[m
[32m+[m[41m    [m
[32m+[m[32m    // Esperar un momento para que se actualice el estado de errores[m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m      console.log("Errores despu√©s de touchAllFields:", errors);[m
[32m+[m[41m      [m
[32m+[m[32m      // Validar todos los campos[m
[32m+[m[32m      const isValid = validateAllFields(values);[m
[32m+[m[32m      console.log("¬øEs v√°lido el formulario?", isValid);[m
[32m+[m[32m      console.log("Errores finales:", errors);[m
[32m+[m[41m      [m
[32m+[m[32m      if (!isValid) {[m
[32m+[m[32m        console.log("Formulario inv√°lido - no se enviar√°");[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      console.log("Formulario v√°lido - enviando datos");[m
[32m+[m[32m      const categoryData = {[m
[32m+[m[32m        Nombre: values.nombreCategoria,[m
[32m+[m[32m        EdadMinima: parseInt(values.edadMinima),[m
[32m+[m[32m        EdadMaxima: parseInt(values.edadMaxima),[m
[32m+[m[32m        Descripcion: values.descripcion,[m
[32m+[m[32m        Estado: values.estado,[m
[32m+[m[32m        Publicar: values.publicar,[m
[32m+[m[32m        Archivo: values.archivo,[m
[32m+[m[32m        id: category?.id || Date.now(),[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      onSave(categoryData);[m
[32m+[m[32m      onClose();[m
[32m+[m[32m    }, 100);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  if (!isOpen) return null;[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="fixed inset-0 flex items-center justify-center bg-black bg-opacity-40 z-50 px-4">[m
[32m+[m[32m      <motion.div[m
[32m+[m[32m        initial={{ opacity: 0, y: -60 }}[m
[32m+[m[32m        animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m        exit={{ opacity: 0, y: -60 }}[m
[32m+[m[32m        transition={{ duration: 0.3 }}[m
[32m+[m[32m        className="bg-white rounded-2xl shadow-2xl p-8 w-full max-w-4xl overflow-y-auto max-h-[90vh]"[m
[32m+[m[32m      >[m
[32m+[m[32m        {/* Header */}[m
[32m+[m[32m        <div className="flex justify-between items-center mb-6">[m
[32m+[m[32m          <h2 className="text-3xl font-bold text-[#9BE9FF]">[m
[32m+[m[32m            {isNew ? "Crear Categor√≠a Deportiva" : "Editar Categor√≠a Deportiva"}[m
[32m+[m[32m          </h2>[m
[32m+[m[32m          <button[m
[32m+[m[32m            onClick={onClose}[m
[32m+[m[32m            className="text-gray-400 hover:text-gray-600 text-2xl font-bold"[m
[32m+[m[32m          >[m
[32m+[m[32m            ‚úñ[m
[32m+[m[32m          </button>[m
[32m+[m[32m        </div>[m
[32m+[m
[32m+[m[32m        {/* Body - Formulario */}[m
[32m+[m[32m        <div className="mt-6 grid grid-cols-1 md:grid-cols-2 gap-6">[m
[32m+[m[32m          {/* Nombre categor√≠a */}[m
[32m+[m[32m          <div className="flex flex-col">[m
[32m+[m[32m            <label className="mb-2 font-medium text-gray-700">[m
[32m+[m[32m              Nombre categor√≠a <span className="text-red-500">*</span>[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="text"[m
[32m+[m[32m              value={values.nombreCategoria}[m
[32m+[m[32m              onChange={(e) => handleChange("nombreCategoria", e.target.value)}[m
[32m+[m[32m              onBlur={() => handleBlur("nombreCategoria")}[m
[32m+[m[32m              placeholder="Nombre de la categor√≠a"[m
[32m+[m[32m              className="w-full border border-gray-300 rounded-xl px-4 py-2 focus:outline-none focus:ring-2 focus:ring-[#9BE9FF] focus:border-transparent"[m
[32m+[m[32m            />[m
[32m+[m[32m            {touched.nombreCategoria && errors.nombreCategoria && ([m
[32m+[m[32m              <div className="flex items-center gap-1 mt-1">[m
[32m+[m[32m                <span className="text-yellow-500">‚ö†</span>[m
[32m+[m[32m                <span className="text-red-500 text-sm">{errors.nombreCategoria}</span>[m
[32m+[m[32m              </div>[m
[32m+[m[32m            )}[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* Edad m√≠nima */}[m
[32m+[m[32m          <div className="flex flex-col">[m
[32m+[m[32m            <label className="mb-2 font-medium text-gray-700">[m
[32m+[m[32m              Edad m√≠nima <span className="text-red-500">*</span>[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="number"[m
[32m+[m[32m              value={values.edadMinima}[m
[32m+[m[32m              onChange={(e) => handleChange("edadMinima", e.target.value)}[m
[32m+[m[32m              onBlur={() => handleBlur("edadMinima")}[m
[32m+[m[32m              placeholder="Edad m√≠nima"[m
[32m+[m[32m              className="w-full border border-gray-300 rounded-xl px-4 py-2 focus:outline-none focus:ring-2 focus:ring-[#9BE9FF] focus:border-transparent"[m
[32m+[m[32m            />[m
[32m+[m[32m            {touched.edadMinima && errors.edadMinima && ([m
[32m+[m[32m              <div className="flex items-center gap-1 mt-1">[m
[32m+[m[32m                <span className="text-yellow-500">‚ö†</span>[m
[32m+[m[32m                <span className="text-red-500 text-sm">{errors.edadMinima}</span>[m
[32m+[m[32m              </div>[m
[32m+[m[32m            )}[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* Edad m√°xima */}[m
[32m+[m[32m          <div className="flex flex-col">[m
[32m+[m[32m            <label className="mb-2 font-medium text-gray-700">[m
[32m+[m[32m              Edad m√°xima <span className="text-red-500">*</span>[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="number"[m
[32m+[m[32m              value={values.edadMaxima}[m
[32m+[m[32m              onChange={(e) => handleChange("edadMaxima", e.target.value)}[m
[32m+[m[32m              onBlur={() => handleBlur("edadMaxima")}[m
[32m+[m[32m              placeholder="Edad m√°xima"[m
[32m+[m[32m              className="w-full border border-gray-300 rounded-xl px-4 py-2 focus:outline-none focus:ring-2 focus:ring-[#9BE9FF] focus:border-transparent"[m
[32m+[m[32m            />[m
[32m+[m[32m            {touched.edadMaxima && errors.edadMaxima && ([m
[32m+[m[32m              <div className="flex items-center gap-1 mt-1">[m
[32m+[m[32m                <span className="text-yellow-500">‚ö†</span>[m
[32m+[m[32m                <span className="text-red-500 text-sm">{errors.edadMaxima}</span>[m
[32m+[m[32m              </div>[m
[32m+[m[32m            )}[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* Descripci√≥n */}[m
[32m+[m[32m          <div className="flex flex-col">[m
[32m+[m[32m            <label className="mb-2 font-medium text-gray-700">[m
[32m+[m[32m              Descripci√≥n[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <textarea[m
[32m+[m[32m              value={values.descripcion}[m
[32m+[m[32m              onChange={(e) => handleChange("descripcion", e.target.value)}[m
[32m+[m[32m              onBlur={() => handleBlur("descripcion")}[m
[32m+[m[32m              placeholder="Descripci√≥n de la categor√≠a"[m
[32m+[m[32m              rows="3"[m
[32m+[m[32m              className="w-full border border-gray-300 rounded-xl px-4 py-2 focus:outline-none focus:ring-2 focus:ring-[#9BE9FF] focus:border-transparent resize-none"[m
[32m+[m[32m            />[m
[32m+[m[32m            {touched.descripcion && errors.descripcion && ([m
[32m+[m[32m              <div className="flex items-center gap-1 mt-1">[m
[32m+[m[32m                <span className="text-yellow-500">‚ö†</span>[m
[32m+[m[32m                <span className="text-red-500 text-sm">{errors.descripcion}</span>[m
[32m+[m[32m              </div>[m
[32m+[m[32m            )}[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* Subir archivo */}[m
[32m+[m[32m          <div className="flex flex-col">[m
[32m+[m[32m            <label className="mb-2 font-medium text-gray-700">[m
[32m+[m[32m              Subir archivo <span className="text-red-500">*</span>[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="file"[m
[32m+[m[32m              onChange={handleFileChange}[m
[32m+[m[32m              onBlur={() => handleBlur("archivo")}[m
[32m+[m[32m              className="block w-full text-sm text-gray-600 file:mr-4 file:py-2 file:px-4[m[41m [m
[32m+[m[32m                         file:rounded-full file:border-0[m[41m [m
[32m+[m[32m                         file:text-sm file:font-semibold[m
[32m+[m[32m                         file:bg-[#9BE9FF] file:text-gray-900[m[41m [m
[32m+[m[32m                         hover:file:bg-[#80dfff] transition"[m
[32m+[m[32m            />[m
[32m+[m[32m            {touched.archivo && errors.archivo && ([m
[32m+[m[32m              <div className="flex items-center gap-1 mt-1">[m
[32m+[m[32m                <span className="text-yellow-500">‚ö†</span>[m
[32m+[m[32m                <span className="text-red-500 text-sm">{errors.archivo}</span>[m
[32m+[m[32m              </div>[m
[32m+[m[32m            )}[m
[32m+[m[32m            {values.archivo && ([m
[32m+[m[32m              <motion.span[m
[32m+[m[32m                initial={{ opacity: 0 }}[m
[32m+[m[32m                animate={{ opacity: 1 }}[m
[32m+[m[32m                className="mt-2 text-sm text-green-600"[m
[32m+[m[32m              >[m
[32m+[m[32m                {typeof values.archivo === 'object' ? values.archivo.name : values.archivo}[m
[32m+[m[32m              </motion.span>[m
[32m+[m[32m            )}[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* Estado */}[m
[32m+[m[32m          <div className="flex flex-col">[m
[32m+[m[32m            <label className="mb-2 font-medium text-gray-700">[m
[32m+[m[32m              Estado <span className="text-red-500">*</span>[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <select[m
[32m+[m[32m              value={values.estado}[m
[32m+[m[32m              onChange={(e) => handleChange("estado", e.target.value)}[m
[32m+[m[32m              onBlur={() => handleBlur("estado")}[m
[32m+[m[32m              className="w-full border border-gray-300 rounded-xl px-4 py-2 focus:outline-none focus:ring-2 focus:ring-[#9BE9FF] focus:border-transparent"[m
[32m+[m[32m            >[m
[32m+[m[32m              <option value="">Seleccione estado</option>[m
[32m+[m[32m              <option value="Activo">Activo</option>[m
[32m+[m[32m              <option value="Inactivo">Inactivo</option>[m
[32m+[m[32m            </select>[m
[32m+[m[32m            {touched.estado && errors.estado && ([m
[32m+[m[32m              <div className="flex items-center gap-1 mt-1">[m
[32m+[m[32m                <span className="text-yellow-500">‚ö†</span>[m
[32m+[m[32m                <span className="text-red-500 text-sm">{errors.estado}</span>[m
[32m+[m[32m              </div>[m
[32m+[m[32m            )}[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* Publicar */}[m
[32m+[m[32m          <div className="flex items-center gap-2 col-span-2">[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="checkbox"[m
[32m+[m[32m              name="publicar"[m
[32m+[m[32m              checked={values.publicar}[m
[32m+[m[32m              onChange={(e) => handleChange("publicar", e.target.checked)}[m
[32m+[m[32m              className="w-4 h-4 text-[#9BE9FF] focus:ring-[#9BE9FF] border-gray-300 rounded"[m
[32m+[m[32m            />[m
[32m+[m[32m            <label className="text-sm font-medium text-gray-700">[m
[32m+[m[32m              Publicar categor√≠a[m
[32m+[m[32m            </label>[m
[32m+[m[32m          </div>[m
[32m+[m[32m        </div>[m
[32m+[m
[32m+[m[32m        {/* Botones */}[m
[32m+[m[32m        <div className="mt-8 flex justify-end gap-4">[m
[32m+[m[32m          <button[m
[32m+[m[32m            type="button"[m
[32m+[m[32m            onClick={onClose}[m
[32m+[m[32m            className="px-5 py-2 rounded-lg bg-gray-200 text-gray-700 font-medium hover:bg-gray-300 transition"[m
[32m+[m[32m          >[m
[32m+[m[32m            Cancelar[m
[32m+[m[32m          </button>[m
[32m+[m[32m          <button[m
[32m+[m[32m            type="button"[m
[32m+[m[32m            onClick={handleSubmit}[m
[32m+[m[32m            className="px-5 py-2 rounded-lg bg-[#9BE9FF] text-gray-900 font-semibold hover:bg-[#80dfff] transition"[m
[32m+[m[32m          >[m
[32m+[m[32m            {isNew ? "Crear" : "Actualizar"}[m
[32m+[m[32m          </button>[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </motion.div>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default SportsCategoryModal;[m
