[1m+++ b/src/features/dashboard/pages/Admin/components/perfilLog.jsx[m
[36m@@ -0,0 +1,50 @@[m
[32m+[m[32mimport React, { useState, useRef, useEffect } from "react";[m
[32m+[m[32mimport { FaAngleDown } from 'react-icons/fa';[m
[32m+[m[32mimport { AnimatePresence } from "framer-motion";[m
[32m+[m[32mimport SubMenu from "./subMenu";[m
[32m+[m
[32m+[m[32mconst PerfilLog = () => {[m
[32m+[m[32m    const [isOpen, setIsOpen] = useState(false);[m
[32m+[m[32m    const menuRef = useRef(null);[m
[32m+[m
[32m+[m[32m    const toggleMenu = () => setIsOpen(!isOpen);[m
[32m+[m
[32m+[m[32m    // Cierra el men√∫ si se hace clic fuera de √©l[m
[32m+[m[32m    useEffect(() => {[m
[32m+[m[32m        const handleClickOutside = (event) => {[m
[32m+[m[32m            if (menuRef.current && !menuRef.current.contains(event.target)) {[m
[32m+[m[32m                setIsOpen(false);[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m[32m        document.addEventListener("mousedown", handleClickOutside);[m
[32m+[m[32m        return () => {[m
[32m+[m[32m            document.removeEventListener("mousedown", handleClickOutside);[m
[32m+[m[32m        };[m
[32m+[m[32m    }, [menuRef]);[m
[32m+[m
[32m+[m[32m    return ([m
[32m+[m[32m        <div ref={menuRef} className="relative">[m
[32m+[m[32m            <button[m
[32m+[m[32m                onClick={toggleMenu}[m
[32m+[m[32m                aria-label="Abrir men√∫ de usuario"[m
[32m+[m[32m                className="flex items-center gap-2 p-1 rounded-full hover:bg-gray-100 transition-colors duration-200 focus:outline-none focus:ring-2 focus:ring-primary-purple/50"[m
[32m+[m[32m            >[m
[32m+[m[32m                {/* Avatar con inicial */}[m
[32m+[m[32m                <div className="w-9 h-9 rounded-full bg-gradient-to-br from-primary-purple to-primary-blue flex items-center justify-center text-white font-bold text-sm">[m
[32m+[m[32m                    E[m
[32m+[m[32m                </div>[m
[32m+[m[32m                {/* Flecha desplegable */}[m
[32m+[m[32m                <FaAngleDown[m
[32m+[m[32m                    size={16}[m
[32m+[m[32m                    className={`text-gray-500 shrink-0 transition-transform duration-300 ${isOpen ? "rotate-180" : ""}`}[m
[32m+[m[32m                />[m
[32m+[m[32m            </button>[m
[32m+[m
[32m+[m[32m            {/* Submen√∫ con animaci√≥n */}[m
[32m+[m[32m            <AnimatePresence>[m
[32m+[m[32m                {isOpen && <SubMenu />}[m
[32m+[m[32m            </AnimatePresence>[m
[32m+[m[32m        </div>[m
[32m+[m[32m    );[m
[32m+[m[32m}[m
[32m+[m[32mexport default PerfilLog;[m
\ No newline at end of file[m
