[36m@@ -0,0 +1,28 @@[m
[32m+[m[32mimport { Navigate, Outlet } from "react-router-dom";[m[41m[m
[32m+[m[32mimport { useAuth } from "../shared/contexts/authContext.jsx";[m[41m[m
[32m+[m[41m[m
[32m+[m[32mconst PrivateRoute = ({ allowedRoles, children }) => {[m[41m[m
[32m+[m[32m  const { isAuthenticated, userRole } = useAuth();[m[41m[m
[32m+[m[41m  [m
[32m+[m[32m  console.log("PrivateRoute - isAuthenticated:", isAuthenticated);[m[41m[m
[32m+[m[32m  console.log("PrivateRoute - userRole:", userRole);[m[41m[m
[32m+[m[32m  console.log("PrivateRoute - allowedRoles:", allowedRoles);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Si no est√° autenticado, redirigir al login[m[41m[m
[32m+[m[32m  if (!isAuthenticated) {[m[41m[m
[32m+[m[32m    console.log("No autenticado, redirigiendo a login");[m[41m[m
[32m+[m[32m    return <Navigate to="/login" />;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Verificar si el rol del usuario est√° en la lista de roles permitidos[m[41m[m
[32m+[m[32m  if (!allowedRoles.includes(userRole)) {[m[41m[m
[32m+[m[32m    console.log("Rol no autorizado, redirigiendo a unauthorized");[m[41m[m
[32m+[m[32m    return <Navigate to="/unauthorized" />;[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  console.log("Acceso permitido para el rol:", userRole);[m[41m[m
[32m+[m[32m  // Si hay children, devolver los children, de lo contrario devolver Outlet[m[41m[m
[32m+[m[32m  return children ? children : <Outlet />;[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32mexport default PrivateRoute;[m[41m[m
