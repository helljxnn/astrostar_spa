[1mdiff --git a/src/features/dashboard/pages/Admin/pages/Users/hooks/useFormUserValidation.js b/src/features/dashboard/pages/Admin/pages/Users/hooks/useFormUserValidation.js[m
[1mnew file mode 100644[m
[1mindex 0000000..6532bda[m
[1m--- /dev/null[m
[1m+++ b/src/features/dashboard/pages/Admin/pages/Users/hooks/useFormUserValidation.js[m
[36m@@ -0,0 +1,257 @@[m
[32m+[m[32mimport { useState } from "react";[m
[32m+[m
[32m+[m[32mexport const useFormUserValidation = (initialValues, validationRules) => {[m
[32m+[m[32m  const [values, setValues] = useState(initialValues);[m
[32m+[m[32m  const [errors, setErrors] = useState({});[m
[32m+[m[32m  const [touched, setTouched] = useState({});[m
[32m+[m
[32m+[m[32m  const validateField = (name, value) => {[m
[32m+[m[32m    const rules = validationRules[name];[m
[32m+[m[32m    if (!rules) return '';[m
[32m+[m
[32m+[m[32m    for (const rule of rules) {[m
[32m+[m[32m      const error = rule(value, values);[m
[32m+[m[32m      if (error) return error;[m
[32m+[m[32m    }[m
[32m+[m[32m    return '';[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const validateAllFields = () => {[m
[32m+[m[32m    const newErrors = {};[m
[32m+[m[32m    Object.keys(validationRules).forEach(name => {[m
[32m+[m[32m      const error = validateField(name, values[name]);[m
[32m+[m[32m      if (error) newErrors[name] = error;[m
[32m+[m[32m    });[m
[32m+[m[32m    setErrors(newErrors);[m
[32m+[m[32m    return Object.keys(newErrors).length === 0;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleChange = (nameOrEvent, value) => {[m
[32m+[m[32m    let name, val;[m
[32m+[m[41m    [m
[32m+[m[32m    // Detectar si es un evento o par√°metros directos[m
[32m+[m[32m    if (typeof nameOrEvent === 'string') {[m
[32m+[m[32m      // Llamada directa: onChange(name, value)[m
[32m+[m[32m      name = nameOrEvent;[m
[32m+[m[32m      val = value;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // Evento: onChange(event)[m
[32m+[m[32m      name = nameOrEvent.target.name;[m
[32m+[m[32m      val = nameOrEvent.target.value;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    setValues(prev => ({ ...prev, [name]: val }));[m
[32m+[m[41m    [m
[32m+[m[32m    if (touched[name]) {[m
[32m+[m[32m      const error = validateField(name, val);[m
[32m+[m[32m      setErrors(prev => ({ ...prev, [name]: error }));[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleBlur = (nameOrEvent) => {[m
[32m+[m[32m    let name;[m
[32m+[m[41m    [m
[32m+[m[32m    // Detectar si es un evento o nombre directo[m
[32m+[m[32m    if (typeof nameOrEvent === 'string') {[m
[32m+[m[32m      // Llamada directa: onBlur(name)[m
[32m+[m[32m      name = nameOrEvent;[m
[32m+[m[32m    } else if (nameOrEvent && nameOrEvent.target) {[m
[32m+[m[32m      // Evento: onBlur(event)[m
[32m+[m[32m      name = nameOrEvent.target.name;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // Llamada sin par√°metros: onBlur()[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    setTouched(prev => ({ ...prev, [name]: true }));[m
[32m+[m[32m    const error = validateField(name, values[name]);[m
[32m+[m[32m    setErrors(prev => ({ ...prev, [name]: error }));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const resetForm = () => {[m
[32m+[m[32m    setValues(initialValues);[m
[32m+[m[32m    setErrors({});[m
[32m+[m[32m    setTouched({});[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    values,[m
[32m+[m[32m    errors,[m
[32m+[m[32m    touched,[m
[32m+[m[32m    handleChange,[m
[32m+[m[32m    handleBlur,[m
[32m+[m[32m    validateAllFields,[m
[32m+[m[32m    setValues,[m
[32m+[m[32m    setErrors,[m
[32m+[m[32m    setTouched,[m
[32m+[m[32m    resetForm[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport const userValidationRules = {[m
[32m+[m[32m  nombre: [[m
[32m+[m[32m    (value) => (!value?.trim() ? "El nombre es obligatorio" : ""),[m
[32m+[m[32m    (value) =>[m
[32m+[m[32m      value?.trim().length < 2 ? "El nombre debe tener al menos 2 caracteres" : "",[m
[32m+[m[32m    (value) =>[m
[32m+[m[32m      value?.trim().length > 50 ? "El nombre no puede exceder 50 caracteres" : "",[m
[32m+[m[32m    (value) =>[m
[32m+[m[32m      !/^[a-zA-Z√°√©√≠√≥√∫√Å√â√ç√ì√ö√±√ë\s]+$/.test(value?.trim() || "")[m
[32m+[m[32m        ? "El nombre solo puede contener letras y espacios"[m
[32m+[m[32m        : "",[m
[32m+[m[32m    (value) => {[m
[32m+[m[32m      const trimmed = value?.trim() || "";[m
[32m+[m[32m      return /\s{2,}/.test(trimmed) ? "No se permiten espacios dobles" : "";[m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m[41m  [m
[32m+[m[32m  apellido: [[m
[32m+[m[32m    (value) => (!value?.trim() ? "El apellido es obligatorio" : ""),[m
[32m+[m[32m    (value) =>[m
[32m+[m[32m      value?.trim().length < 2 ? "El apellido debe tener al menos 2 caracteres" : "",[m
[32m+[m[32m    (value) =>[m
[32m+[m[32m      value?.trim().length > 50 ? "El apellido no puede exceder 50 caracteres" : "",[m
[32m+[m[32m    (value) =>[m
[32m+[m[32m      !/^[a-zA-Z√°√©√≠√≥√∫√Å√â√ç√ì√ö√±√ë\s]+$/.test(value?.trim() || "")[m
[32m+[m[32m        ? "El apellido solo puede contener letras y espacios"[m
[32m+[m[32m        : "",[m
[32m+[m[32m    (value) => {[m
[32m+[m[32m      const trimmed = value?.trim() || "";[m
[32m+[m[32m      return /\s{2,}/.test(trimmed) ? "No se permiten espacios dobles" : "";[m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m
[32m+[m[32m  tipoDocumento: [(value) => (!value ? "Debe seleccionar un tipo de documento" : "")],[m
[32m+[m
[32m+[m[32m  identificacion: [[m
[32m+[m[32m    (value) => (!value?.trim() ? "La identificaci√≥n es obligatoria" : ""),[m
[32m+[m[32m    (value) => {[m
[32m+[m[32m      const cleaned = value?.trim().replace(/\s/g, '') || "";[m
[32m+[m[32m      return !/^\d+$/.test(cleaned) ? "La identificaci√≥n debe contener solo n√∫meros" : "";[m
[32m+[m[32m    },[m
[32m+[m[32m    (value, allValues) => {[m
[32m+[m[32m      const cleaned = value?.trim().replace(/\s/g, '') || "";[m
[32m+[m[32m      const tipoDoc = allValues?.tipoDocumento;[m
[32m+[m[41m      [m
[32m+[m[32m      if (!tipoDoc || !cleaned) return "";[m
[32m+[m[41m      [m
[32m+[m[32m      // Validaciones espec√≠ficas por tipo de documento colombiano[m
[32m+[m[32m      switch (tipoDoc) {[m
[32m+[m[32m        case "CC": // C√©dula de Ciudadan√≠a[m
[32m+[m[32m          if (cleaned.length < 6 || cleaned.length > 10) {[m
[32m+[m[32m            return "La c√©dula debe tener entre 6 y 10 d√≠gitos";[m
[32m+[m[32m          }[m
[32m+[m[32m          break;[m
[32m+[m[32m        case "TI": // Tarjeta de Identidad[m
[32m+[m[32m          if (cleaned.length < 10 || cleaned.length > 11) {[m
[32m+[m[32m            return "La tarjeta de identidad debe tener entre 10 y 11 d√≠gitos";[m
[32m+[m[32m          }[m
[32m+[m[32m          break;[m
[32m+[m[32m        case "CE": // C√©dula de Extranjer√≠a[m
[32m+[m[32m          if (cleaned.length < 6 || cleaned.length > 12) {[m
[32m+[m[32m            return "La c√©dula de extranjer√≠a debe tener entre 6 y 12 d√≠gitos";[m
[32m+[m[32m          }[m
[32m+[m[32m          break;[m
[32m+[m[32m        case "PAS": // Pasaporte[m
[32m+[m[32m          if (cleaned.length < 6 || cleaned.length > 15) {[m
[32m+[m[32m            return "El pasaporte debe tener entre 6 y 15 caracteres";[m
[32m+[m[32m          }[m
[32m+[m[32m          // Los pasaportes pueden tener letras y n√∫meros[m
[32m+[m[32m          if (!/^[A-Z0-9]+$/i.test(value?.trim() || "")) {[m
[32m+[m[32m            return "El pasaporte solo puede contener letras y n√∫meros";[m
[32m+[m[32m          }[m
[32m+[m[32m          break;[m
[32m+[m[32m      }[m
[32m+[m[32m      return "";[m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m
[32m+[m[32m  rol: [(value) => (!value ? "Debe seleccionar un rol" : "")],[m
[32m+[m
[32m+[m[32m  correo: [[m
[32m+[m[32m    (value) => (!value?.trim() ? "El correo es obligatorio" : ""),[m
[32m+[m[32m    (value) => {[m
[32m+[m[32m      const email = value?.trim() || "";[m
[32m+[m[32m      // Validaci√≥n m√°s estricta de email[m
[32m+[m[32m      const emailRegex = /^[a-zA-Z0-9]([a-zA-Z0-9._-])*[a-zA-Z0-9]@[a-zA-Z0-9]([a-zA-Z0-9.-])*[a-zA-Z0-9]\.[a-zA-Z]{2,}$/;[m
[32m+[m[32m      return !emailRegex.test(email) ? "El correo electr√≥nico no es v√°lido" : "";[m
[32m+[m[32m    },[m
[32m+[m[32m    (value) => {[m
[32m+[m[32m      const email = value?.trim() || "";[m
[32m+[m[32m      return email.length > 100 ? "El correo no puede exceder 100 caracteres" : "";[m
[32m+[m[32m    },[m
[32m+[m[32m    (value) => {[m
[32m+[m[32m      const email = value?.trim() || "";[m
[32m+[m[32m      // Verificar dominios comunes malformados[m
[32m+[m[32m      const commonDomains = ['gmail.com', 'hotmail.com', 'yahoo.com', 'outlook.com', 'live.com'];[m
[32m+[m[32m      const domain = email.split('@')[1]?.toLowerCase();[m
[32m+[m[32m      if (domain && commonDomains.includes(domain.replace(/[^a-z.]/g, ''))) {[m
[32m+[m[32m        return !commonDomains.includes(domain) ? `¬øQuisiste decir @${domain.replace(/[^a-z.]/g, '')}?` : "";[m
[32m+[m[32m      }[m
[32m+[m[32m      return "";[m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m
[32m+[m[32m  telefono: [[m
[32m+[m[32m    (value) => (!value?.trim() ? "El n√∫mero telef√≥nico es obligatorio" : ""),[m
[32m+[m[32m    (value) => {[m
[32m+[m[32m      let phone = value?.trim() || "";[m
[32m+[m[41m      [m
[32m+[m[32m      // Limpiar el n√∫mero removiendo espacios, guiones y par√©ntesis[m
[32m+[m[32m      const cleanPhone = phone.replace(/[\s\-\(\)]/g, '');[m
[32m+[m[41m      [m
[32m+[m[32m      // Si ya tiene +57 o 57 al inicio, validar como est√°[m
[32m+[m[32m      if (cleanPhone.startsWith('+57') || cleanPhone.startsWith('57')) {[m
[32m+[m[32m        const localNumber = cleanPhone.replace(/^(\+57|57)/, '');[m
[32m+[m[41m        [m
[32m+[m[32m        // Validar n√∫mero local despu√©s del c√≥digo de pa√≠s[m
[32m+[m[32m        if (localNumber.length === 10 && /^3/.test(localNumber)) {[m
[32m+[m[32m          return ""; // Celular v√°lido[m
[32m+[m[32m        }[m
[32m+[m[32m        if (localNumber.length === 7 && /^[2-8]/.test(localNumber)) {[m
[32m+[m[32m          return ""; // Fijo v√°lido[m
[32m+[m[32m        }[m
[32m+[m[32m        return "N√∫mero inv√°lido. Celular: 3XXXXXXXXX (10 d√≠gitos), Fijo: 2XXXXXXX-8XXXXXXX (7 d√≠gitos)";[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      // Si no tiene c√≥digo de pa√≠s, asumir que es n√∫mero local colombiano[m
[32m+[m[32m      if (!/^\d+$/.test(cleanPhone)) {[m
[32m+[m[32m        return "El tel√©fono solo puede contener n√∫meros";[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      // Validar n√∫mero local (se asume +57 autom√°ticamente)[m
[32m+[m[32m      if (cleanPhone.length === 10 && /^3/.test(cleanPhone)) {[m
[32m+[m[32m        return ""; // Celular colombiano v√°lido (se agregar√° +57 autom√°ticamente)[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      if (cleanPhone.length === 7 && /^[2-8]/.test(cleanPhone)) {[m
[32m+[m[32m        return ""; // Tel√©fono fijo colombiano v√°lido (se agregar√° +57 autom√°ticamente)[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      // Mensajes de error espec√≠ficos[m
[32m+[m[32m      if (cleanPhone.length < 7) {[m
[32m+[m[32m        return "El n√∫mero debe tener al menos 7 d√≠gitos";[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      if (cleanPhone.length > 10) {[m
[32m+[m[32m        return "N√∫mero demasiado largo. M√°ximo 10 d√≠gitos para celular";[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      if (cleanPhone.length === 10 && !/^3/.test(cleanPhone)) {[m
[32m+[m[32m        return "Los n√∫meros celulares deben iniciar con 3";[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      if (cleanPhone.length === 7 && !/^[2-8]/.test(cleanPhone)) {[m
[32m+[m[32m        return "Los n√∫meros fijos deben iniciar con 2, 3, 4, 5, 6, 7 u 8";[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      if (cleanPhone.length === 8 || cleanPhone.length === 9) {[m
[32m+[m[32m        return "Longitud inv√°lida. Use 7 d√≠gitos (fijo) o 10 d√≠gitos (celular)";[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      return "Formato de tel√©fono inv√°lido";[m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m
[32m+[m[32m  estado: [(value) => (!value ? "Debe seleccionar un estado" : "")][m
[32m+[m[32m};[m
