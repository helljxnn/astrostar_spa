[32m+[m[32m  subMonths,[m
[32m+[m[32m  addWeeks,[m
[32m+[m[32m  subWeeks,[m
[32m+[m[32m  addDays,[m
[32m+[m[32m  subDays,[m
[32m+[m[32m} from "date-fns";[m
[32m+[m[32mimport es from "date-fns/locale/es";[m
[32m+[m[32mimport { motion, AnimatePresence } from "framer-motion";[m
