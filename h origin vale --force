[32m+[m[32mimport React from "react";[m
[32m+[m[32mimport { FaUserTie } from "react-icons/fa";[m
[32m+[m[32mimport PerfilLog from "./perfilLog";[m
[32m+[m[32mimport { useAuth } from "../../../../../shared/contexts/authContext.jsx";[m
[32m+[m[32mimport { motion } from "framer-motion";[m
[32m+[m
[32m+[m[32mexport const TopBar = () => {[m
[32m+[m[32m  const { userRole } = useAuth();[m
[32m+[m
[32m+[m[32m  // Funci√≥n para formatear el rol para mostrar[m
[32m+[m[32m  const formatRole = (role) => {[m
[32m+[m[32m    if (!role) return "";[m
[32m+[m
[32m+[m[32m    // Convertir formato snake_case a T√≠tulo Capitalizado[m
[32m+[m[32m    const words = role.split("_");[m
[32m+[m[32m    return words[m
[32m+[m[32m      .map((word) => word.charAt(0).toUpperCase() + word.slice(1))[m
[32m+[m[32m      .join(" ");[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <header[m
[32m+[m[32m      className="w-full h-16 px-6 flex items-center justify-between[m[41m [m
[32m+[m[32m      bg-gradient-to-r from-white/70 via-white/60 to-white/70[m[41m [m
[32m+[m[32m      backdrop-blur-lg shadow-md sticky top-0 z-40 border-b border-gray-200 font-montserrat"[m
[32m+[m[32m    >[m
[32m+[m[32m      {/* Rol */}[m
[32m+[m[32m      <motion.div[m
[32m+[m[32m        className="flex items-center gap-3 cursor-default"[m
[32m+[m[32m        initial={{ opacity: 0, y: -10 }}[m
[32m+[m[32m        animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m        transition={{ duration: 0.5 }}[m
[32m+[m[32m      >[m
[32m+[m[32m        <motion.div[m
[32m+[m[32m          whileHover={{ scale: 1.15, rotate: 5 }}[m
[32m+[m[32m          transition={{ type: "spring", stiffness: 300 }}[m
[32m+[m[32m          className="p-2 rounded-full bg-primary-purple/10 text-primary-purple shadow-sm"[m
[32m+[m[32m        >[m
[32m+[m[32m          <FaUserTie size={20} />[m
[32m+[m[32m        </motion.div>[m
[32m+[m[32m        <motion.h4[m
[32m+[m[32m          className="text-gray-800 font-semibold tracking-wide text-lg"[m
[32m+[m[32m          initial={{ opacity: 0 }}[m
[32m+[m[32m          animate={{ opacity: 1 }}[m
[32m+[m[32m          transition={{ delay: 0.2 }}[m
[32m+[m[32m        >[m
[32m+[m[32m          {formatRole(userRole)}[m
[32m+[m[32m        </motion.h4>[m
[32m+[m[32m      </motion.div>[m
[32m+[m
[32m+[m[32m      {/* Submenu perfil */}[m
[32m+[m[32m      <motion.div[m
[32m+[m[32m        className="relative"[m
[32m+[m[32m        initial={{ opacity: 0, x: 15 }}[m
[32m+[m[32m        animate={{ opacity: 1, x: 0 }}[m
[32m+[m[32m        transition={{ delay: 0.3, duration: 0.5 }}[m
[32m+[m[32m      >[m
[32m+[m[32m        <PerfilLog />[m
[32m+[m[32m      </motion.div>[m
[32m+[m[32m    </header>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
