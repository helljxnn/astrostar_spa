[36m@@ -0,0 +1,125 @@[m
[32m+[m[32m  "use client";[m
[32m+[m
[32m+[m[32m  import { useState, useMemo, useEffect, useCallback, useRef } from "react";[m
[32m+[m[32m  import {[m
[32m+[m[32m    eventsData,[m
[32m+[m[32m    eventTypes,[m
[32m+[m[32m    processEventsStatus,[m
[32m+[m[32m    filterEventsByType,[m
[32m+[m[32m    getUpcomingEvents,[m
[32m+[m[32m    getPastEvents,[m
[32m+[m[32m    getNextEvent,[m
[32m+[m[32m  } from "../../../../../shared/utils/helpers/eventsHelper";[m
[32m+[m
[32m+[m[32m  export const useEvents = () => {[m
[32m+[m[32m    const [selectedEventType, setSelectedEventType] = useState("todos");[m
[32m+[m[32m    const [selectedEventId, setSelectedEventId] = useState(null);[m
[32m+[m[32m    const [selectedDate, setSelectedDate] = useState(null);[m
[32m+[m
[32m+[m[32m    const previousNextEventId = useRef(null);[m
[32m+[m[32m    const previousEventTypeRef = useRef(selectedEventType);[m
[32m+[m
[32m+[m[32m    // üîπ Procesar todos los eventos con su estado[m
[32m+[m[32m    const processedEvents = useMemo(() => {[m
[32m+[m[32m      return processEventsStatus(eventsData);[m
[32m+[m[32m    }, []);[m
[32m+[m
[32m+[m[32m    // üîπ Eventos filtrados por tipo[m
[32m+[m[32m    const filteredEvents = useMemo(() => {[m
[32m+[m[32m      return filterEventsByType(processedEvents, selectedEventType);[m
[32m+[m[32m    }, [processedEvents, selectedEventType]);[m
[32m+[m
[32m+[m[32m    // üîπ Pr√≥ximos eventos[m
[32m+[m[32m    const upcomingEvents = useMemo(() => {[m
[32m+[m[32m      return getUpcomingEvents(filteredEvents);[m
[32m+[m[32m    }, [filteredEvents]);[m
[32m+[m
[32m+[m[32m    // üîπ Eventos pasados[m
[32m+[m[32m    const pastEvents = useMemo(() => {[m
[32m+[m[32m      return getPastEvents(filteredEvents);[m
[32m+[m[32m    }, [filteredEvents]);[m
[32m+[m
[32m+[m[32m    // üîπ Pr√≥ximo evento[m
[32m+[m[32m    const nextEvent = useMemo(() => {[m
[32m+[m[32m      return getNextEvent(upcomingEvents);[m
[32m+[m[32m    }, [upcomingEvents]);[m
[32m+[m
[32m+[m[32m    // üîπ Cambiar tipo de evento[m
[32m+[m[32m    const handleEventTypeChange = useCallback((newType) => {[m
[32m+[m[32m      setSelectedEventType(newType);[m
[32m+[m[32m      setSelectedDate(null);[m
[32m+[m[32m    }, []);[m
[32m+[m
[32m+[m[32m    // üîπ Selecci√≥n de evento[m
[32m+[m[32m    const handleEventSelect = useCallback((eventId) => {[m
[32m+[m[32m      setSelectedEventId(eventId);[m
[32m+[m[32m    }, []);[m
[32m+[m
[32m+[m[32m    // üîπ Selecci√≥n de fecha[m
[32m+[m[32m    const handleDateSelect = useCallback((date) => {[m
[32m+[m[32m      setSelectedDate(date);[m
[32m+[m[32m    }, []);[m
[32m+[m
[32m+[m[32m    // üîπ Mantener actualizado el evento seleccionado seg√∫n nextEvent[m
[32m+[m[32m    useEffect(() => {[m
[32m+[m[32m      const nextEventId = nextEvent?.id || null;[m
[32m+[m
[32m+[m[32m      if (previousNextEventId.current !== nextEventId) {[m
[32m+[m[32m        setSelectedEventId(nextEventId);[m
[32m+[m[32m        previousNextEventId.current = nextEventId;[m
[32m+[m
[32m+[m[32m        if (nextEvent) {[m
[32m+[m[32m          try {[m
[32m+[m[32m            setSelectedDate(new Date(nextEvent.date));[m
[32m+[m[32m          } catch (error) {[m
[32m+[m[32m            console.error("‚ùå Error parseando fecha:", error);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }, [nextEvent]);[m
[32m+[m
[32m+[m[32m    // üîπ Reset al cambiar tipo de evento[m
[32m+[m[32m    useEffect(() => {[m
[32m+[m[32m      if (previousEventTypeRef.current !== selectedEventType) {[m
[32m+[m[32m        previousEventTypeRef.current = selectedEventType;[m
[32m+[m
[32m+[m[32m        const timeoutId = setTimeout(() => {[m
[32m+[m[32m          if (nextEvent) {[m
[32m+[m[32m            setSelectedEventId(nextEvent.id);[m
[32m+[m[32m            try {[m
[32m+[m[32m              setSelectedDate(new Date(nextEvent.date));[m
[32m+[m[32m            } catch (error) {[m
[32m+[m[32m              console.error("‚ùå Error parseando fecha:", error);[m
[32m+[m[32m            }[m
[32m+[m[32m          } else {[m
[32m+[m[32m            setSelectedEventId(null);[m
[32m+[m[32m            setSelectedDate(null);[m
[32m+[m[32m          }[m
[32m+[m[32m        }, 50);[m
[32m+[m
[32m+[m[32m        return () => clearTimeout(timeoutId);[m
[32m+[m[32m      }[m
[32m+[m[32m    }, [selectedEventType, nextEvent]);[m
[32m+[m
[32m+[m[32m    // üîπ Evento seleccionado global[m
[32m+[m[32m    const selectedEvent = useMemo(() => {[m
[32m+[m[32m      if (!selectedEventId) return null;[m
[32m+[m[32m      return processedEvents.find((event) => event.id === selectedEventId) || null;[m
[32m+[m[32m    }, [selectedEventId, processedEvents]);[m
[32m+[m
[32m+[m[32m    return {[m
[32m+[m[32m      eventTypes,[m
[32m+[m[32m      selectedEventType,[m
[32m+[m[32m      setSelectedEventType: handleEventTypeChange,[m
[32m+[m[32m      upcomingEvents,[m
[32m+[m[32m      pastEvents,[m
[32m+[m[32m      nextEvent,[m
[32m+[m[32m      allEvents: processedEvents,[m[41m [m
[32m+[m[32m      filteredEvents,[m
[32m+[m[32m      selectedEvent,[m
[32m+[m[32m      selectedEventId,[m
[32m+[m[32m      setSelectedEventId: handleEventSelect,[m
[32m+[m[32m      selectedDate,[m
[32m+[m[32m      setSelectedDate: handleDateSelect,[m
[32m+[m[32m    };[m
[32m+[m[32m  };[m
[1mdiff --git a/astrostar_spa/src/features/landing/pages/Home.jsx b/src/features/landing/pages/Home.jsx[m
[1msimilarity index 100%[m
[1mrename from astrostar_spa/src/features/landing/pages/Home.jsx[m
[1mrename to src/features/landing/pages/Home.jsx[m
