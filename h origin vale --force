[32m+[m[32mimport { useState, useEffect } from "react";[m
[32m+[m[32mimport { motion } from "framer-motion";[m
[32m+[m[32mimport { FormField } from "../../../../../../../../shared/components/FormField";[m
[32m+[m[32mimport { useFormScheduleValidation } from "../hooks/useFormSchedulealidation";[m
[32m+[m[32mimport { showSuccessAlert } from "../../../../../../../../shared/utils/alerts";[m
[32m+[m[32mimport CustomRecurrenceModal from "./CustomRecurrenceModal";[m
[32m+[m
[32m+[m[32m// üîπ Mock temporal de empleados[m
[32m+[m[32mconst empleadosMock = [[m
[32m+[m[32m  { id: 1, nombre: "Juan P√©rez", cargo: "Entrenador" },[m
[32m+[m[32m  { id: 2, nombre: "Ana G√≥mez", cargo: "Nutricionista" },[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32mexport default function ScheduleModal({ onClose, onSave, schedule, isNew }) {[m
[32m+[m[32m  // ---------------- STATE ----------------[m
[32m+[m[32m  const [showCustomModal, setShowCustomModal] = useState(false);[m
[32m+[m[32m  const [form, setForm] = useState({[m
[32m+[m[32m    empleadoId: "",[m
[32m+[m[32m    empleado: "",[m
[32m+[m[32m    cargo: "",[m
[32m+[m[32m    fecha: "",[m
[32m+[m[32m    horaInicio: "",[m
[32m+[m[32m    horaFin: "",[m
[32m+[m[32m    repeticion: "no",[m
[32m+[m[32m    customRecurrence: null,[m
[32m+[m[32m    descripcion: "",[m
[32m+[m[32m    estado: "Programado",[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  const { errors, touched, validate, handleBlur, touchAllFields } =[m
[32m+[m[32m    useFormScheduleValidation();[m
[32m+[m
[32m+[m[32m  // ---------------- EFFECTS ----------------[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    if (!isNew && schedule) {[m
[32m+[m[32m      setForm({[m
[32m+[m[32m        empleadoId: schedule.empleadoId || "",[m
[32m+[m[32m        empleado: schedule.empleado || "",[m
[32m+[m[32m        cargo: schedule.cargo || "",[m
[32m+[m[32m        fecha: schedule.fecha || "",[m
[32m+[m[32m        horaInicio: schedule.horaInicio || "",[m
[32m+[m[32m        horaFin: schedule.horaFin || "",[m
[32m+[m[32m        repeticion: schedule.repeticion || "no",[m
[32m+[m[32m        customRecurrence: schedule.customRecurrence || null,[m
[32m+[m[32m        descripcion: schedule.descripcion || "",[m
[32m+[m[32m        estado: schedule.estado || "Programado",[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }, [schedule, isNew]);[m
[32m+[m
[32m+[m[32m  // ---------------- HANDLERS ----------------[m
[32m+[m[32m  const handleChange = (name, value) => {[m
[32m+[m[32m    if (name === "repeticion" && value === "personalizado") {[m
[32m+[m[32m      setShowCustomModal(true);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      setForm((prev) => ({ ...prev, [name]: value }));[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleSubmit = () => {[m
[32m+[m[32m    touchAllFields(form);[m
[32m+[m[32m    if (validate(form)) {[m
[32m+[m[32m      onSave({ ...form, id: schedule?.id || Date.now() });[m
[32m+[m[32m      showSuccessAlert([m
[32m+[m[32m        isNew[m
[32m+[m[32m          ? "Horario de empleado creado exitosamente"[m
[32m+[m[32m          : "Horario de empleado actualizado exitosamente"[m
[32m+[m[32m      );[m
[32m+[m[32m      onClose();[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // ---------------- RENDER ----------------[m
[32m+[m[32m  return ([m
[32m+[m[32m    <>[m
[32m+[m[32m      {/* Modal principal */}[m
[32m+[m[32m      <div className="fixed inset-0 flex items-center justify-center bg-black/40 z-50 px-4">[m
[32m+[m[32m        <motion.div[m
[32m+[m[32m          initial={{ opacity: 0, y: -60 }}[m
[32m+[m[32m          animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m          exit={{ opacity: 0, y: -60 }}[m
[32m+[m[32m          transition={{ duration: 0.3 }}[m
[32m+[m[32m          className="bg-white rounded-2xl shadow-2xl p-8 w-full max-w-3xl overflow-y-auto max-h-[90vh]"[m
[32m+[m[32m        >[m
[32m+[m[32m          {/* Header */}[m
[32m+[m[32m          <div className="flex justify-between items-center mb-6">[m
[32m+[m[32m            <h2 className="text-3xl font-bold text-[#9BE9FF]">[m
[32m+[m[32m              {isNew ? "Crear Horario" : "Editar Horario"}[m
[32m+[m[32m            </h2>[m
[32m+[m[32m            <button[m
[32m+[m[32m              onClick={onClose}[m
[32m+[m[32m              className="text-gray-400 hover:text-gray-600 text-2xl font-bold"[m
[32m+[m[32m            >[m
[32m+[m[32m              ‚úñ[m
[32m+[m[32m            </button>[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* Formulario */}[m
[32m+[m[32m          <div className="grid grid-cols-1 md:grid-cols-2 gap-6">[m
[32m+[m[32m            {/* Empleado */}[m
[32m+[m[32m            <div>[m
[32m+[m[32m              <label className="block text-sm font-medium text-gray-700 mb-2">[m
[32m+[m[32m                Empleado[m
[32m+[m[32m              </label>[m
[32m+[m[32m              <select[m
[32m+[m[32m                value={form.empleadoId}[m
[32m+[m[32m                onChange={(e) => {[m
[32m+[m[32m                  const emp = empleadosMock.find([m
[32m+[m[32m                    (x) => x.id === Number(e.target.value)[m
[32m+[m[32m                  );[m
[32m+[m[32m                  setForm((prev) => ({[m
[32m+[m[32m                    ...prev,[m
[32m+[m[32m                    empleadoId: emp?.id || "",[m
[32m+[m[32m                    empleado: emp?.nombre || "",[m
[32m+[m[32m                    cargo: emp?.cargo || "",[m
[32m+[m[32m                  }));[m
[32m+[m[32m                }}[m
[32m+[m[32m                onBlur={() => handleBlur("empleado", form.empleado, form)}[m
[32m+[m[32m                className="w-full border rounded-lg px-3 py-2 focus:ring-2 focus:ring-[#9BE9FF] outline-none"[m
[32m+[m[32m                required[m
[32m+[m[32m              >[m
[32m+[m[32m                <option value="">-- Selecciona un empleado --</option>[m
[32m+[m[32m                {empleadosMock.map((emp) => ([m
[32m+[m[32m                  <option key={emp.id} value={emp.id}>[m
