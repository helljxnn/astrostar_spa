[1mdiff --git a/src/shared/contexts/authContext.jsx b/src/shared/contexts/authContext.jsx[m
[1mnew file mode 100644[m
[1mindex 0000000..68e1308[m
[1m--- /dev/null[m
[1m+++ b/src/shared/contexts/authContext.jsx[m
[36m@@ -0,0 +1,67 @@[m
[32m+[m[32mimport { createContext, useContext, useState, useEffect } from "react";[m
[32m+[m
[32m+[m[32mconst AuthContext = createContext();[m
[32m+[m
[32m+[m[32mexport const AuthProvider = ({ children }) => {[m
[32m+[m[32m  const [isAuthenticated, setIsAuthenticated] = useState(false);[m
[32m+[m[32m  const [userRole, setUserRole] = useState(null);[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    const token = localStorage.getItem("authToken");[m
[32m+[m[32m    const role = localStorage.getItem("userRole");[m
[32m+[m[32m    if (token) {[m
[32m+[m[32m      setIsAuthenticated(true);[m
[32m+[m[32m      setUserRole(role);[m
[32m+[m[32m      console.log("AuthContext - Sesi√≥n restaurada:", { token, role });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      console.log("AuthContext - No hay sesi√≥n guardada");[m
[32m+[m[32m    }[m
[32m+[m[32m  }, []);[m
[32m+[m
[32m+[m[32mconst login = (userData) => {[m
[32m+[m[32m  console.log("AuthContext - Intentando login con:", userData);[m
[32m+[m[41m  [m
[32m+[m[32m  // Definir usuarios y sus roles[m
[32m+[m[32m  const users = [[m
[32m+[m[32m    { email: "admin@example.com", password: "admin123", role: "admin" },[m
[32m+[m[32m    { email: "pro@example.com", password: "pro123", role: "profesional_deportivo" },[m
[32m+[m[32m    { email: "salud@example.com", password: "salud123", role: "profesional_salud" },[m
[32m+[m[32m    { email: "deportista@example.com", password: "dep123", role: "deportista" },[m
[32m+[m[32m    { email: "acudiente@example.com", password: "acu123", role: "acudiente" }[m
[32m+[m[32m  ];[m
[32m+[m[41m  [m
[32m+[m[32m  // Buscar el usuario que coincida con las credenciales[m
[32m+[m[32m  const user = users.find(u => u.email === userData.email && u.password === userData.password);[m
[32m+[m[41m  [m
[32m+[m[32m  if (user) {[m
[32m+[m[32m    const token = `fake-${user.role}-token`;[m
[32m+[m[32m    setIsAuthenticated(true);[m
[32m+[m[32m    setUserRole(user.role);[m
[32m+[m[32m    localStorage.setItem("authToken", token);[m
[32m+[m[32m    localStorage.setItem("userRole", user.role);[m
[32m+[m[32m    console.log("AuthContext - Login exitoso:", { role: user.role, token });[m
[32m+[m[32m    return true;[m
[32m+[m[32m  }[m
[32m+[m[41m  [m
[32m+[m[32m  console.log("AuthContext - Login fallido: credenciales incorrectas");[m
[32m+[m[32m  return false;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32m  const logout = () => {[m
[32m+[m[32m    setIsAuthenticated(false);[m
[32m+[m[32m    setUserRole(null);[m
[32m+[m[32m    localStorage.removeItem("authToken");[m
[32m+[m[32m    localStorage.removeItem("userRole");[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <AuthContext.Provider value={{ isAuthenticated, userRole, login, logout }}>[m
[32m+[m[32m      {children}[m
[32m+[m[32m    </AuthContext.Provider>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport const useAuth = () => {[m
[32m+[m[32m  return useContext(AuthContext);[m
[32m+[m[32m};[m
