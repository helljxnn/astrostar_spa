[32m+[m[32mimport { useState, useEffect } from "react";[m[41m[m
[32m+[m[41m[m
[32m+[m[32mexport const useFormRoleValidation = (initialValues, validationRules) => {[m[41m[m
[32m+[m[32m  const [values, setValues] = useState(initialValues);[m[41m[m
[32m+[m[32m  const [errors, setErrors] = useState({});[m[41m[m
[32m+[m[32m  const [touched, setTouched] = useState({});[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const validateField = (name, value) => {[m[41m[m
[32m+[m[32m    const rules = validationRules[name];[m[41m[m
[32m+[m[32m    if (!rules) return '';[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    for (const rule of rules) {[m[41m[m
[32m+[m[32m      const error = rule(value, values);[m[41m[m
[32m+[m[32m      if (error) return error;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    return '';[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const validateAllFields = () => {[m[41m[m
[32m+[m[32m    const newErrors = {};[m[41m[m
[32m+[m[32m    Object.keys(validationRules).forEach(name => {[m[41m[m
[32m+[m[32m      const error = validateField(name, values[name]);[m[41m[m
[32m+[m[32m      if (error) newErrors[name] = error;[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[32m    setErrors(newErrors);[m[41m[m
[32m+[m[32m    return Object.keys(newErrors).length === 0;[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const handleChange = (name, value) => {[m[41m[m
[32m+[m[32m    setValues(prev => ({ ...prev, [name]: value }));[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    if (touched[name]) {[m[41m[m
[32m+[m[32m      const error = validateField(name, value);[m[41m[m
[32m+[m[32m      setErrors(prev => ({ ...prev, [name]: error }));[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const handleBlur = (name) => {[m[41m[m
[32m+[m[32m    setTouched(prev => ({ ...prev, [name]: true }));[m[41m[m
[32m+[m[32m    const error = validateField(name, values[name]);[m[41m[m
[32m+[m[32m    setErrors(prev => ({ ...prev, [name]: error }));[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  const resetForm = () => {[m[41m[m
[32m+[m[32m    setValues(initialValues);[m[41m[m
[32m+[m[32m    setErrors({});[m[41m[m
[32m+[m[32m    setTouched({});[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  return {[m[41m[m
[32m+[m[32m    values,[m[41m[m
[32m+[m[32m    errors,[m[41m[m
[32m+[m[32m    touched,[m[41m[m
[32m+[m[32m    handleChange,[m[41m[m
[32m+[m[32m    handleBlur,[m[41m[m
[32m+[m[32m    validateAllFields,[m[41m[m
[32m+[m[32m    setValues,[m[41m[m
[32m+[m[32m    setErrors,[m[41m[m
[32m+[m[32m    setTouched,[m[41m[m
[32m+[m[32m    resetForm[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[32m// Reglas de validaci√≥n para el formulario de roles[m[41m[m
[32m+[m[32mexport const roleValidationRules = {[m[41m[m
[32m+[m[32m  nombre: [[m[41m[m
[32m+[m[32m    (value) => !value?.trim() ? 'El nombre es obligatorio' : '',[m[41m[m
[32m+[m[32m    (value) => value?.length < 3 ? 'El nombre debe tener al menos 3 caracteres' : '',[m[41m[m
[32m+[m[32m    (value) => value?.length > 50 ? 'El nombre no puede exceder 50 caracteres' : '',[m[41m[m
[32m+[m[32m    (value) => !/^[a-zA-Z√°√©√≠√≥√∫√Å√â√ç√ì√ö√±√ë\s]+$/.test(value || '') ? 'El nombre solo puede contener letras y espacios' : ''[m[41m[m
[32m+[m[32m  ],[m[41m[m
[32m+[m[32m  descripcion: [[m[41m[m
[32m+[m[32m    (value) => !value?.trim() ? 'La descripci√≥n es obligatoria' : '',[m[41m[m
[32m+[m[32m    (value) => value?.length > 200 ? 'La descripci√≥n no puede exceder 200 caracteres' : '',[m[41m[m
[32m+[m[32m  ],[m[41m[m
[32m+[m[32m  estado: [[m[41m[m
[32m+[m[32m    (value) => !value ? 'Debe seleccionar un estado' : ''[m[41m[m
[32m+[m[32m  ][m[41m[m
 };[m
