[36m@@ -0,0 +1,147 @@[m
[32m+[m[32mimport { useState } from "react";[m
[32m+[m[32mimport { motion } from "framer-motion";[m
[32m+[m
[32m+[m[32mconst diasSemana = [[m
[32m+[m[32m  { value: 0, label: "Domingo" },[m
[32m+[m[32m  { value: 1, label: "Lunes" },[m
[32m+[m[32m  { value: 2, label: "Martes" },[m
[32m+[m[32m  { value: 3, label: "Mi√©rcoles" },[m
[32m+[m[32m  { value: 4, label: "Jueves" },[m
[32m+[m[32m  { value: 5, label: "Viernes" },[m
[32m+[m[32m  { value: 6, label: "S√°bado" },[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32mexport default function CustomRecurrenceModal({ isOpen, onClose, onSave }) {[m
[32m+[m[32m  const [frequency, setFrequency] = useState("semana"); // d√≠a, semana, mes, a√±o[m
[32m+[m[32m  const [interval, setInterval] = useState(1);[m
[32m+[m[32m  const [repeticiones, setRepeticiones] = useState(10);[m
[32m+[m[32m  const [dias, setDias] = useState([]); // solo si es personalizado[m
[32m+[m
[32m+[m[32m  if (!isOpen) return null;[m
[32m+[m
[32m+[m[32m  const toggleDia = (value) => {[m
[32m+[m[32m    setDias((prev) =>[m
[32m+[m[32m      prev.includes(value) ? prev.filter((d) => d !== value) : [...prev, value][m
[32m+[m[32m    );[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleSave = () => {[m
[32m+[m[32m    onSave({[m
[32m+[m[32m      repeticion: dias.length > 0 ? "personalizado" : frequency,[m
[32m+[m[32m      dias,[m
[32m+[m[32m      repeticiones,[m
[32m+[m[32m      intervalo: interval,[m
[32m+[m[32m      label:[m
[32m+[m[32m        dias.length > 0[m
[32m+[m[32m          ? `Personalizado: ${dias[m
[32m+[m[32m              .map((d) => diasSemana.find((x) => x.value === d)?.label)[m
[32m+[m[32m              .join(", ")}`[m
[32m+[m[32m          : `Cada ${interval} ${frequency}${interval > 1 ? "s" : ""}`,[m
[32m+[m[32m    });[m
[32m+[m[32m    onClose();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="fixed inset-0 flex items-center justify-center bg-black/40 z-50">[m
[32m+[m[32m      <motion.div[m
[32m+[m[32m        initial={{ opacity: 0, y: -60 }}[m
[32m+[m[32m        animate={{ opacity: 1, y: 0 }}[m
[32m+[m[32m        exit={{ opacity: 0, y: -60 }}[m
[32m+[m[32m        transition={{ duration: 0.3 }}[m
[32m+[m[32m        className="bg-white rounded-2xl shadow-xl p-6 w-full max-w-md"[m
[32m+[m[32m      >[m
[32m+[m[32m        <h2 className="text-xl font-bold text-gray-800 mb-4">[m
[32m+[m[32m          Personalizar repetici√≥n[m
[32m+[m[32m        </h2>[m
[32m+[m
[32m+[m[32m        <div className="space-y-4">[m
[32m+[m[32m          {/* üîπ Frecuencia */}[m
[32m+[m[32m          <div>[m
[32m+[m[32m            <label className="block text-sm font-medium text-gray-700 mb-2">[m
[32m+[m[32m              Frecuencia[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <select[m
[32m+[m[32m              value={frequency}[m
[32m+[m[32m              onChange={(e) => setFrequency(e.target.value)}[m
[32m+[m[32m              className="w-full border rounded-lg px-3 py-2 focus:ring-2 focus:ring-blue-400 outline-none"[m
[32m+[m[32m            >[m
[32m+[m[32m              <option value="dia">D√≠a</option>[m
[32m+[m[32m              <option value="semana">Semana</option>[m
[32m+[m[32m              <option value="mes">Mes</option>[m
[32m+[m[32m              <option value="anio">A√±o</option>[m
[32m+[m[32m              <option value="personalizado">Personalizado</option>[m
[32m+[m[32m            </select>[m
[32m+[m[32m          </div>[m
[32m+[m
[32m+[m[32m          {/* üîπ Intervalo */}[m
[32m+[m[32m          {frequency !== "personalizado" && ([m
[32m+[m[32m            <div>[m
[32m+[m[32m              <label className="block text-sm font-medium text-gray-700 mb-2">[m
[32m+[m[32m                Cada cu√°ntos {frequency}s[m
[32m+[m[32m              </label>[m
[32m+[m[32m              <input[m
[32m+[m[32m                type="number"[m
[32m+[m[32m                min="1"[m
[32m+[m[32m                value={interval}[m
[32m+[m[32m                onChange={(e) => setInterval(Number(e.target.value))}[m
[32m+[m[32m                className="w-full border rounded-lg px-3 py-2 focus:ring-2 focus:ring-blue-400 outline-none"[m
[32m+[m[32m              />[m
[32m+[m[32m            </div>[m
[32m+[m[32m          )}[m
[32m+[m
[32m+[m[32m          {/* üîπ D√≠as espec√≠ficos si es personalizado */}[m
[32m+[m[32m          {frequency === "personalizado" && ([m
[32m+[m[32m            <div>[m
[32m+[m[32m              <p className="font-medium mb-2">Selecciona los d√≠as:</p>[m
[32m+[m[32m              <div className="grid grid-cols-2 gap-2">[m
[32m+[m[32m                {diasSemana.map((dia) => ([m
[32m+[m[32m                  <label[m
[32m+[m[32m                    key={dia.value}[m
[32m+[m[32m                    className="flex items-center gap-2 text-sm"[m
[32m+[m[32m                  >[m
[32m+[m[32m                    <input[m
[32m+[m[32m                      type="checkbox"[m
[32m+[m[32m                      checked={dias.includes(dia.value)}[m
[32m+[m[32m                      onChange={() => toggleDia(dia.value)}[m
[32m+[m[32m                    />[m
[32m+[m[32m                    {dia.label}[m
[32m+[m[32m                  </label>[m
[32m+[m[32m                ))}[m
[32m+[m[32m              </div>[m
[32m+[m[32m            </div>[m
[32m+[m[32m          )}[m
[32m+[m
[32m+[m[32m          {/* üîπ N√∫mero de repeticiones */}[m
[32m+[m[32m          <div>[m
[32m+[m[32m            <label className="block text-sm font-medium text-gray-700 mb-2">[m
[32m+[m[32m              N√∫mero de repeticiones[m
[32m+[m[32m            </label>[m
[32m+[m[32m            <input[m
[32m+[m[32m              type="number"[m
[32m+[m[32m              min="1"[m
[32m+[m[32m              value={repeticiones}[m
[32m+[m[32m              onChange={(e) => setRepeticiones(Number(e.target.value))}[m
[32m+[m[32m              className="w-full border rounded-lg px-3 py-2 focus:ring-2 focus:ring-blue-400 outline-none"[m
[32m+[m[32m            />[m
[32m+[m[32m          </div>[m
[32m+[m[32m        </div>[m
[32m+[m
[32m+[m[32m        {/* Botones */}[m
[32m+[m[32m        <div className="mt-6 flex justify-end gap-3">[m
[32m+[m[32m          <button[m
[32m+[m[32m            onClick={onClose}[m
[32m+[m[32m            className="px-5 py-2 rounded-lg bg-gray-200 text-gray-700 hover:bg-gray-300 transition"[m
[32m+[m[32m          >[m
[32m+[m[32m            Cancelar[m
[32m+[m[32m          </button>[m
[32m+[m[32m          <button[m
[32m+[m[32m            onClick={handleSave}[m
[32m+[m[32m            className="px-5 py-2 rounded-lg bg-blue-500 text-white font-semibold hover:bg-blue-600 transition"[m
[32m+[m[32m          >[m
[32m+[m[32m            Guardar[m
[32m+[m[32m          </button>[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </motion.div>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
